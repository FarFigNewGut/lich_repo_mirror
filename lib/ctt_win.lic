			cttmsgBold("Weapon used:  #{GameObj.right_hand.noun} against AvD: #{$crit_hash['avd']}, d100 Roll:  #{$crit_hash['droll']}, Endroll: #{$crit_hash['endroll_total']}, Total Damage: #{$crit_hash['total_damage']}")
		elsif $crit_hash.keys.include?('cva')
			if $crit_hash.keys.include?('hidden')
				cttmsgBold("Spell cast against CvA:  #{$crit_hash['cva']}, d100 Roll:  #{$crit_hash['droll']}, Endroll: #{$crit_hash['endroll_total']}.\nHidden disruption damage:  #{$crit_hash['hidden']}  with   #{$crit_hash['total_damage']}  total observed damage.")
			else
				cttmsgBold("Spell cast against CvA:  #{$crit_hash['cva']}, d100 Roll:  #{$crit_hash['droll']}, Endroll: #{$crit_hash['endroll_total']}, Total Damage:  #{$crit_hash['total_damage']}")
			end
		else
			cttmsgBold("Unknown damage done:  #{$crit_hash['total_damage']} Total Damage")
		end
		$crit_hash = {}
	end
