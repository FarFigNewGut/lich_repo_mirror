def
	cttmsg(message)
	puts('<pushStream id="CttWindow" ifClosedStyle="watching"/>' + message + "\n<popStream/>"	)
end


 def cttmsgBold(message)
	puts('<pushBold/><pushStream id="CttWindow" ifClosedStyle="watching"/>' + message + "\n\n<popStream/><popBold/>"	)
 end

puts("<streamWindow type='dynamic' id='CttWindow' title='Crit Type Tracker' target='CttWindow' location='main' height='100' width='400' resident='true'></streamWindow>"	)

