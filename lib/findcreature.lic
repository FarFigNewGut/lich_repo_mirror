def self.groupcheck()
  fput "GROUP"
  members = 0
  group = []
  line = get
  until line =~ /for a list of other options./
    if line =~ /^(.*) is following you./
      members += 1
      group.push($1)
    elsif line =~ /^(.*) is also a member of your group./
      members += 1
      group.push($1)
    end
    line = get
  end
  return group
end

