    echo "Creature not added yet or was mispelled, exiting script"
    exit
end

danger_check = (critter_level - Char.level)
echo "Creature information: #{web_link}"
echo "**** Warning **** Creature is #{danger_check} levels higher than you, could be dangerous" if (danger_check > 4) 
pause 3

wander_rooms = Array.new

prefight_stuff = proc {
    if !checkstanding
        until checkstanding
            fput "stand"
            waitrt?
        end
     end
                   
    if checkstance != "defensive" && !checkcastrt
         fput "stance def"
    end

    if percenthealth <= 70
        echo "You are below 70% health, going to heal up"
        waitrt?
        Script.run('go2', 'node  _disable_confirm_')
        echo "You are below 70% health, get healed!"
        echo "#{creatures_killed} #{critter_to_find}s Killed"
        exit
    end
}

new_room_stuff = proc {
        pause 0.3
        someone_is_here = true if GameObj.pcs.find if $group.length < 1
        someone_is_here = true if GameObj.pcs.find { |pc| pc.name !~ /\b(?:#{$group.join('|')})/ } if $group.length > 0
        someone_is_here = true if GameObj.loot.find { |loot| loot.name =~ /disk|coffin/i && loot.name !~ /\b(?:#{XMLData.name}|#{$group.join('|')}) (?:disk|coffin)/ }
        someone_is_here = true if $obvious_hiding_player

        if someone_is_here
            echo "Room is occupied, moving on!"
            echo "Hidden player detected" if $obvious_hiding_player
        else
            if @active_settings['stop_for_all_creatures'] == true
                target = GameObj.npcs.find{ |npc| npc.type =~ /aggressive/i }
            else
                target = GameObj.npcs.find{ |npc| npc.noun.downcase == critter_noun && npc.name.downcase != "#{wrong_critter}" } if !alternate_targeting_method
                target = GameObj.npcs.find{ |npc| npc.name.downcase =~ /#{critter_to_find}/ && npc.name.downcase != "#{wrong_critter}" } if alternate_targeting_method
                target = GameObj.npcs.find{ |npc| npc.name =~ /#{critter_to_find}/ && npc.name =~ /grizzled|ancient/ } if checkbounty =~ /You have provoked/
            end

            if target
                echo "target aquired"
                fput "target ##{target.id}"

                wait_until { target.status =~ /dead|gone/i or percenthealth < 70 }
                
                if target.status =~ /dead/
                    creatures_killed = (creatures_killed + 1)
                    waitrt?
                    if @active_settings['loot_creatures'] == true
                        Script.run("sloot")
                    else
                        echo "Auto loot is not enabled, you must skin\/search\/loot manualy"
                        wait_until { target.status =~ /gone/i }
                        echo "Move any direction when you are finished looting to continue"
                        last_room = Room.current.id
                        wait_until { last_room != Room.current.id }
                    end
                end

                prefight_stuff.call

            end

            if target
                new_room_stuff.call
            end
        end

        someone_is_here = nil
}

new_room_stuff.call

loop {
    room = Room.current
    acceptable_adjacent_rooms = room.wayto.keys & rooms
    not_visited_rooms = acceptable_adjacent_rooms.find_all { |r| not wander_rooms.include?(r) }
    
    society_spells_to_keep_on.each { |i| Spell[i].cast if Spell[i].known? && Spell[i].affordable? && !Spell[i].active?} if @active_settings['use_society_spells'] == true
    fput "stance def" if checkstance != "defensive" && !checkcastrt

    if not_visited_rooms.empty?
         next_room = wander_rooms.find { |r| acceptable_adjacent_rooms.include?(r) }
    else
        next_room = not_visited_rooms[rand(not_visited_rooms.length)]
    end

    if next_room
        wander_rooms.delete(next_room)
        wander_rooms.push(next_room)
        way = room.wayto[next_room]

        if way.class == String
            move(way)
            new_room_stuff.call
        else
            way.call
            new_room_stuff.call
        end
    else
        start_script 'go2', [ Room.current.find_nearest(rooms.collect { |id| id.to_i }).to_s ]
        wait_while { running?('go2') }
        new_room_stuff.call
    end

    $obvious_hiding_player = false
}
