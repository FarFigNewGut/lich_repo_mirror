before_dying {
    DownstreamHook.remove("hiden_person_check")
    total_time_running = (Time.now.to_i - time_started)
    echo( sprintf( "Time Running\: %d hours %d minutes %0.0f seconds", total_time_running/60/60, total_time_running/60%60, total_time_running%60 ) )
    echo "Creatures killed\: #{creatures_killed}"
    }

CharSettings.load
CharSettings['find_creature_first_time_run'] = nil unless CharSettings['find_creature_first_time_run']
settings = CharSettings.to_hash
reset = nil

if script.vars[1] =~ /^setup$|^options$/i or !CharSettings['find_creature_first_time_run']
    CharSettings['find_creature_first_time_run'] = true
    window = window_action = nil
    
    Gtk.queue {
        gtk_checks = Hash.new
        
        saveButton = Gtk::Button.new("_Save and Close")
    
        window = Gtk::Window.new(Gtk::Window::TOPLEVEL)
        window.set_title  "Find Creature by: Nugt"
        window.border_width = 20

        vbox = Gtk::VBox.new(false, 0)
        
        use_society_spells_check = Gtk::CheckButton.new("Use society spells\?")
        use_society_spells_check.active = settings['use_society_spells']
        gtk_checks['use_society_spells'] = use_society_spells_check

        loot_creatures_check = Gtk::CheckButton.new("Automacialy loot creatures\?")
        loot_creatures_check.active = settings['loot_creatures']
        gtk_checks['loot_creatures'] = loot_creatures_check    

        stop_for_all_creatures_check = Gtk::CheckButton.new("Stop for all critters\?")
        stop_for_all_creatures_check.active = settings['stop_for_all_creatures']
        gtk_checks['stop_for_all_creatures'] = stop_for_all_creatures_check 

        tooltips = Gtk::Tooltips.new
        tooltips.set_tip(use_society_spells_check, "Check to enable auto use of society spells", "")
        tooltips.set_tip(loot_creatures_check, "Check to use sloot automaticlly when you kill a creature", "")
        tooltips.set_tip(stop_for_all_creatures_check, "Stop for any creature in the area you are searching", "")

        hb = Gtk::HBox.new(false,1)
        hb.pack_start(use_society_spells_check.set_width_request(180), false, true, 0)
        hb.pack_start(loot_creatures_check.set_width_request(180), false, true, 0)
        vbox.pack_start(hb,false)

        hb = Gtk::HBox.new(false,1)
        hb.pack_start(stop_for_all_creatures_check.set_width_request(180), false, true, 0)
        vbox.pack_start(hb,false)

        vbox.pack_start(saveButton)
        
        window.add(vbox)
        saveButton.signal_connect('clicked') {
            Gtk.queue {
                gtk_checks.each_pair { 
                    |number,check| settings[number] = check.active? 
                }
            
                window_action = :save
            }
        }
            
        window.signal_connect('delete_event') {window_action = :cancel}
        window.show_all
        window.resizable = false
    }

    before_dying { Gtk.queue { window.destroy } }
    wait_while { window_action.nil? }
    undo_before_dying
    Gtk.queue { window.destroy }

    Script.self.kill
end

