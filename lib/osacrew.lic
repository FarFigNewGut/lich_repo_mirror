def stealth_disabler_routine
	if $osa_stealth_disabler == 0
		stance_defensive
		fput "search"
		waitrt?
		waitcastrt?
	elsif $osa_stealth_disabler == 1
		$stealth_disabler = "Dispel Invisibility"
		cast_disabler
	elsif $osa_stealth_disabler == 2
		$stealth_disabler = "Searing Light"
		cast_disabler
	elsif $osa_stealth_disabler == 3
		$stealth_disabler = "Light"
		cast_disabler
	elsif $osa_stealth_disabler == 4
		$stealth_disabler = "Censure"
		cast_disabler
	elsif $osa_stealth_disabler == 5
		$stealth_disabler = "Divine Wrath"
		cast_disabler
	elsif $osa_stealth_disabler == 6
		$stealth_disabler = "Elemental Wave"
		cast_disabler
	elsif $osa_stealth_disabler == 7
		$stealth_disabler = "Major Elemental Wave"
		cast_disabler
	elsif $osa_stealth_disabler == 8
		$stealth_disabler = "Cone of Elements"
		cast_disabler
	elsif $osa_stealth_disabler == 9
		$stealth_disabler = "Sun Burst"
		cast_disabler
	elsif $osa_stealth_disabler == 10
		$stealth_disabler = "Nature's Fury"
		cast_disabler
	elsif $osa_stealth_disabler == 11
		$stealth_disabler = "Grasp of the Grave"
		cast_disabler
	elsif $osa_stealth_disabler == 12
		$stealth_disabler = "Implosion"
		cast_disabler
	elsif $osa_stealth_disabler == 13
		$stealth_disabler = "Tremors"
		cast_disabler
	elsif $osa_stealth_disabler == 14
		$stealth_disabler = "Call Wind"
		cast_disabler
	elsif $osa_stealth_disabler == 15
		$stealth_disabler = "Aura of the Arkati"
		cast_disabler
	elsif $osa_stealth_disabler == 16
		$stealth_disabler = "Judgement"
		cast_disabler
	elsif $osa_stealth_disabler == 17
		stance_offensive
		fput "cman eviscerate"
		stance_defensive
	elsif $osa_stealth_disabler == 18
		stance_offensive
		Warcry.use("Cry", "All")
		stance_defensive
	elsif $osa_stealth_disabler == 19
		stance_defensive
		fput "symbol of sleep"
		waitrt?
		waitcastrt?
	end
end

def cast_disabler
	if !Spell["#{$stealth_disabler}"].known?
		respond ""
		respond "You Have Selected A Stealth Diabling Spell You Do Not Know, Please Select One Known To Your Character. Defaulting To Search"
		respond ""
		return
		fput "search"
		waitrt?
		waitcastrt?
	else
		fput "prep #{$stealth_disabler}"
		fput "cast"
		waitrt?
		waitcastrt?
		if $stealth_disabler == "Light"
			fput "search"
			waitrt?
			waitcastrt?
		end
	end
end

