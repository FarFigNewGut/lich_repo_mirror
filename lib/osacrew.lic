def recieve_bless
result = matchtimeout 15, /a moment and then gently dissipates|leaving a soft white afterglow|appears to become incorporated into it|but it quickly returns to normal/
	if result.to_s =~ /a moment and then gently dissipates|leaving a soft white afterglow|appears to become incorporated into it|but it quickly returns to normal/
		return
	else
		respond ""
		respond "                                  Something May Have Gone Wrong With The Bless                               "
		respond ""
	end
end

def get_bless
	if $osa_needbless
		LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_blesser }, "I Need Blessed Please!")
		waitfor /^\[#{$osa_crew}\]-GSIV:#{$osa_blesser}\: \"#{checkname}/
		if $osa_uachands.empty? and $osa_uacfeet.empty?
			fput "gird"
			pause 1
		else
			if !$osa_uachands.empty?
				fput "remove #{$osa_uachands}"
				pause 0.5
			end
			if !$osa_uacfeet.empty?
				fput "remove #{$osa_uacfeet}"
			end
		end
		if !GameObj.left_hand.id.nil? && GameObj.right_hand.id.nil?
			fput "swap"
		end
		if !GameObj.left_hand.id.nil? && !GameObj.right_hand.id.nil?
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_blesser }, "I Have Two.")
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_blesser }, "I Am Ready.")
			recieve_bless
			fput "swap"
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_blesser }, "Ok, The Next One Is Ready.")
			recieve_bless
		else
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_blesser }, "I Have One.")
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_blesser }, "I Am Ready.")
			recieve_bless
		end
		if $osa_uachands.empty? and $osa_uacfeet.empty?
			fput "store both"
		else
			if !$osa_uachands.empty?
				fput "wear #{$osa_uachands}"
				pause 0.5
			end
			if !$osa_uacfeet.empty?
				fput "wear #{$osa_uacfeet}"
			end
		end	
	end
end

def cast_bless
	result = matchtimeout 5, /^\[Private\]-GSIV:#{@name}\: \"I Am Ready.\"|^\[Private\]-GSIV:#{@name}\: \"Ok, The Next One Is Ready.\"/
		if result.to_s =~ /^\[Private\]-GSIV:#{@name}\: \"I Am Ready.\"|^\[Private\]-GSIV:#{@name}\: \"Ok, The Next One Is Ready.\"/
			if Spell[1604].known? and Spell[1604].affordable?
				Spell[1604].cast("#{@name}")
			end
			waitcastrt?
			if Spell[304].known? and Spell[304].affordable?
				Spell[304].cast("#{@name}")
			else
				fput "symbol bless #{@name}"
			end
		end
end
		
def give_bless
	LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "#{@name}")
	result = matchtimeout 5, /^\[Private\]-GSIV:#{@name}\: \"I Have One.\"|^\[Private\]-GSIV:#{@name}\: \"I Have Two.\"/
		if result.to_s =~ /^\[Private\]-GSIV:#{@name}\: \"I Have One.\"/
			cast_bless
		elsif result.to_s =~ /^\[Private\]-GSIV:#{@name}\: \"I Have Two.\"/
			2.times{cast_bless}
		end
end

def who_needs_blessed
	result = matchtimeout 1, /^\[Private\]-GSIV:(.*)\: \"I Need Blessed Please!\"/
	if result.to_s =~ /^\[Private\]-GSIV:(.*)\: \"I Need Blessed Please!\"/
		@blessname.push($1)
		who_needs_blessed
	end
end

def begin_bless
	if (Spell[304].known? or Spell[9802].known?) && $osa_givebless == true
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Will Be Providing All Crew Blessings!")
		@blessname = Array.new
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Does Anyone Need A Bless?")
		who_needs_blessed
		@blessname.each do |n|
			@name = n
			give_bless
		end
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "The Crew Has Been Properly Blessed!")
	else
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Can Anyone Bless?")
		result = matchtimeout 3, /^\[Private\]-GSIV:(.*)\: \"I Can Captain!\"/
		if result.to_s =~ /^\[Private\]-GSIV:(.*)\: \"I Can Captain!\"/
			$osa_blesser = $1
			LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "#{$osa_blesser}, Will You Please Bless The Crew?")
			waitfor /^\[#{$osa_crew}\]-GSIV:#{$osa_blesser}\: \"Does Anyone Need A Bless\?\"/
			get_bless
			waitfor /^\[#{$osa_crew}\]-GSIV:#{$osa_blesser}\: \"The Crew Has Been Properly Blessed Captain!"/
		else
			LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "We Do Not Have Anyone Present Who Can Bless The Crew, We Will Continue Without!")
		end
	end		
end

