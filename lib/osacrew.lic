	if @winded
		pause 5
		return
	else
		helm
		return
	end
end

def winded_sails
	if @winded
		main_deck
		cast_sails
	else
		helm
		return
	end
	if @winded
		if UserVars.osacrew["ship_map"].include? "mid_deck"
			mid_deck
			cast_sails
		end
	else
		helm
		return
	end
	if @winded
		if UserVars.osacrew["ship_map"].include? "forward_deck"
			forward_deck
			cast_sails
		end
	else
		helm
		return
	end
	10.times{check_winded}
	winded_sails
end

