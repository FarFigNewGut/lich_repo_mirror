@sunk = false
@stop = false
$osa_depart = false
$osa_piratehunter = false
$osa_logging = false
@medicalofficer_patient = Array.new
@supportlist = Array.new
@severlist = Array.new
if $osa_stealth_disabler.nil?
	$osa_stealth_disabler = 0
end
if $osa_boarding.nil?
	$osa_boarding = false
end
if checkname != $osa_commander
	$osa_cleanup = false
	$osa_piratehunter = false
end
#if $osa_crewsize.to_s.empty?
#	$osa_crewsize = Array.new
#end
#if $osa_crewsize.to_s.nil?
#	$osa_crewsize = Array.new
#end
if script.vars[1].nil?
	crew_display_settings
end
Room[30129].tags.push("meta:trashcan:bucket")
Room[30175].tags.push("meta:trashcan:bucket")
Room[30180].tags.push("meta:trashcan:bucket")
Room[30124].tags.push("meta:trashcan:bucket")
Room[30140].tags.push("meta:trashcan:bucket")
Room[29042].tags.push("meta:trashcan:bucket")

before_dying {
if (checkname == ($osa_commander)) && !$osa_crew.empty?
	if script.vars[1] != /settings/
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "*Ding* *Ding* #{$osa_commander_ship_name} Departing!") if !$osa_crew.empty?
	end
else
	if (!script.vars[1] != /settings/) && !$osa_crew.empty?
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "*Ding* *Ding* Crewman #{checkname} Departing!") if !$osa_crew.empty?
	end
end
}

Thread.new{
	loop
		begin
		command = upstream_get
			if command =~/^\<c\>;osacrew setup/i
				self.crewsetup()
				nil
			elsif command =~/^(\<c\>;osacrew help|\<c\>;osacrew ?)/i
				crew_help_display
				nil
			end
		end}

while line = get
	case line
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_blesser}\: \"Does Anyone Need A Bless\?\"/
		if running? "osacommander"
			next
		end
		if $osa_needbless
			get_bless
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}: "Enemy Vessel Detected, (.*) Sound General Quarters! Gunners Man Your Irons!"/
		if $1.downcase.include? "ethereal"
			$osa_enemy_type = "undead"
		end
		if $1.downcase.include? "krolvin"
			$osa_enemy_type = "krolvin"
		end
		if $1.downcase.include? "dark"
			$osa_enemy_type = "pirate"
		end
		ship_map
		if $osa_cannoneer == true
			@boarded = false
			@thirty = false
			@sunk = false
			@stop = false
			do_client ';e while thirty = get;if thirty.match?(/^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Stop\"/);@stop = true;exit;elsif thirty.match?(/The sides of the (.*) collide against your (.*)/);@boarded = true;exit;elsif thirty.match?(/Tenebrous Cauldron.  Victory is yours!/);@sunk = true;exit;elsif thirty.match?(/^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Thirty second warning, drop what yer doing and prepare for battle. Here they come!"/);@thirty = true;exit;end;end'
			gunner_cycle
		else
			if $osa_osacombat == true
				if !running? "osacombat" 
					start_script "osacombat"
				end
			else
				echo "You Are Not Currently In A Combatant Role, Ready Thyself For Combat!"
				if checkname != "Tearieyes"
					do_client "gird"
				else
					fput "get stick from my sling"
				end
				if $osa_medicalofficer.include? "#{checkname}"
						captains_quarters
				end
			end
		end	
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}: "Enemy Vessel Detected, (.*) Inbound. Sound General Quarters!"/
		if $1.downcase.include? "ethereal"
			$osa_enemy_type = "undead"
			$osa_creature_type = "undead"
		end
		if $1.downcase.include? "krolvin"
			$osa_enemy_type = "krolvin"
			$osa_creature_type = "living"
		end
		if $1.downcase.include? "dark"
			$osa_enemy_type = "pirate"
			$osa_creature_type = "living"
		end
		ship_map
		if $osa_windedsails
			@winded = true
			do_client ';e while winded = get;if winded.match?(/(.*) Is Now Moored In (.*). Estimated Trip Time Was Approximately/);@winded = false;exit;elsif winded.match?(/The sides of the (.*) collide against your (.*)/);@winded = false;exit;elsif winded.match?(/^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Stop\"/);@winded = false;exit;elsif winded.match?(/^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Thirty second warning, drop what yer doing and prepare for battle. Here they come!"/);@winded = false;exit;end;end'
			winded_sails
			wait_until {checkpcs.include? $osa_commander}
			fput "join #{$osa_commander}"
			@winded = true
		end
		if $osa_osacombat == true
			if !running? "osacombat"
				start_script "osacombat"
			end
		else
			echo "You Are Not Currently In A Combatant Role, Ready Thyself For Combat!"
			if checkname != "Tearieyes"
				do_client "gird"
			else
				fput "get stick from my sling"
			end
			if $osa_medicalofficer.include? "#{checkname}"
				captains_quarters
			end
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}: "(.*) Expects To Make Way From/
		if $osa_windedsails
			@winded = true
			do_client ";e while winded = get;if winded.match?(/(.*) Is Now Moored In (.*). Estimated Trip Time Was Approximately/);@winded = false;exit;end;end"
			winded_sails
			wait_until {checkpcs.include? $osa_commander}
			fput "join #{$osa_commander}"
			@winded = true
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Turn To!\"/
		ship_type
		wait_until { !running?("eloot") }
		if $osa_osacombat == true
				stop_script "osacombat"
				wait_while { running?("osacombat") }
		end
		waitrt?
		waitcastrt?
		if !GameObj.left_hand.id.nil? or !GameObj.right_hand.id.nil?
			fput "store both"
		end
		fput "leave"
		pause 0.5
		fput "group open"
		if Room.current.tags.include? "captains_quarters"
			nil
		else
			damage_control
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Stop\"/
		if $osa_osacombat == true
			if running? "osacombat"
				stop_script "osacombat"
				wait_while { running?("osacombat") }
			end
			waitrt?
			waitcastrt?
			fput "store both"
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"All Hands Make Ready To Get Underway!\"/
		ship_map
		if $osa_osacrewtasks == true
			fput "leave"
			pause 0.5
			fput "group open"
			main_deck
			if $osa_ship_type == "sloop" 
				one_mast
			elsif ($osa_ship_type == "brigantine" or $osa_ship_type == "carrack" or $osa_ship_type == "galleon" or $osa_ship_type == "frigate")
				two_masts
			elsif $osa_ship_type == "man o' war"
				three_masts
			end
			#move "go door"
			captains_quarters
		else
			captains_quarters
			echo "You Are Not Currently In A Crew Role, Please Standby To Standby!" 
		end
		pause 1
		if percentmana <= 84
			respond ""
			respond "          -----------------------------------------------------"
			respond "          |                Waiting For Mana                   |"
			respond "          -----------------------------------------------------"
			respond ""
			wait_until {percentmana >= 85}
		end
		pause 5
		wait_until {Room.current.tags.include? "captains_quarters"}
		crew_task_complete
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Status Report\"/
		status_check
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Resource Report\"/
		resource_check
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Gemstone Report\"/
		gemstone_check
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Spells\"/
		crew_spell_up
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Crew, Spell Up (.*).\"/
		$pc = $1
		crew_spell_individual
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Task Time!\"/
		crew_check_task
		
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Deposit\"/
		fput "depo all"
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Sheath\"/
		do_client "sheath"
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Reset\"/
		do_client ";e stop_script 'osacrew';wait_while { running?('osacrew') };start_script 'osacrew'"
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Change Of Command: (.*)\"/
		$osa_commander = $1
		respond "
		Your New Commanding Officer Is Now: #{$osa_commander}
		"
		when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Crew Swap: (.*)\"/
		do_client ";lnet untune #{$osa_crew}"
		pause 0.5
		$save_new_data = Hash.new
		$save_new_data.store(:$osa_crew, $1)
		self.save_script_data()
		pause 0.5
		do_client ";lnet tune #{$osa_crew}"
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Pause\"/
		if $osa_osacombat == true
			if running? "osacombat"
				pause_script "osacombat"
			end
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Unpause\"/
		if $osa_osacombat == true
			if running? "osacombat"
				unpause_script "osacombat"
			end
		end
	when /^\[#{$osa_crew}\]-GSIV:(.*)\: \"I Need (.*) Mana!\"$/
		@manaperson = $1
		$mana_type_needed = Array.new
		$mana_type_needed.clear
		replacements = {'or ' => ' ', ' ' => '', ',' => ' '}
		$mana_type_needed = $2.gsub(Regexp.union(replacements.keys), replacements).split(' ')
		$mana_type_needed.each do |n|
			$matched_type = true if $my_mana_type.include? (n)
		end
		crew_send_mana
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Make Repairs!\"/
		damage_control
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Can Anyone Bless\?\"/
		if $osa_givebless
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_commander }, "I Can Captain!")
			result = matchtimeout 3, /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"#{checkname}, Will You Please Bless The Crew\?"/
				if result.to_s =~ /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"#{checkname}, Will You Please Bless The Crew\?"/
					LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Of Course Captain!")
					@blessname = Array.new
					LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Does Anyone Need A Bless?")
					who_needs_blessed
					@blessname.each do |n|
						@name = n
						give_bless
					end
					LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "The Crew Has Been Properly Blessed Captain!")
				else
					next
				end
		else
			next
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"(.*), Will You Please Bless The Crew\?\"/
		$osa_blesser = $1
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"I Will Be Providing All Crew Blessings!\"/
		$osa_blesser = $osa_commander
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Quarters! All Hands To Quarters For Muster, Instruction and Inspection!\"/
			pause rand(1..5)
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_commander }, "Crewman #{checkname} Reporting For Duty Captain!")
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Current Version Are As Follows(.)/
		start_script("osacombat", ["version"])
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "My Versions are as follows: Combat #{$osa_combatversion} and Crew #{$osa_crewversion}")
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}: "Crew, Report To: (.*)\"/
		if !checkpcs.include? "#{$osa_commander}"
			start_script("go2", [$1])
			wait_while { running?("go2") }
		end
		if checkpcs.include? "#{$osa_commander}"
			fput "join #{$osa_commander}"
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}: "Mana Spellup\"/
		if (Stats.prof != "Warrior") and (Stats.prof !="Rogue")
			fput "mana spellup"
		end
	when /^\[#{$osa_crew}\]-GSIV:(.*): "\*Ding\* \*Ding\* Crewman (.*) Departing!\"/
		if checkname == ($osa_commander) and $osa_crewsize.include? "#{$2}" and !$osa_logging
			respond "
			Removing #{$2} From The Ship's Roster!
			"
			$osa_crewsize.delete "#{$2}"
			#do_client ";var set crewsize=#{@crewsize.count}"
		end
	when /^\[#{$osa_crew}\]-GSIV:(.*): "Crewman (.*), Returning For Duty Captain!\"/
		if checkname == ($osa_commander) and !$osa_crewsize.include? "#{$2}"
			respond "
			Adding #{$2} To The Ship's Roster!
			"
			$osa_crewsize.push ($2)
			#do_client ";var set crewsize=#{@crewsize.count}"
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_medicalofficer}: "I Shall Supply The Bread!\"/
		if ($osa_groupspellup or $osa_selfspellup)
			fput "stow all"
			pause rand(1..3)
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_medicalofficer }, "I Will Take Some Please.")
			crew_recieve_bread
		end
	when /^\[#{$osa_crew}\]-(GSIV:#{$osa_commander}|You): "Let Us Break Bread Together!\"/
		if $osa_medicalofficer.include? "#{checkname}"
			LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Shall Supply The Bread!")
			@breadlist = Array.new
			medicalofficer_bread_orders
			multifput "stow all", "incant 203"
			crew_eat_bread
			@breadlist.each do |n|
				@person = n
				medicalofficer_bread
			end
			LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Bread Is Served!")
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}: "Connection Test: #{checkname}\"/
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Test Satisfactory Captain!")
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}: "Lay Below Crewman #{checkname}!\"/
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Lay Below, Aye Captain!")
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "*Ding* *Ding* Crewman #{checkname} Departing!") if !$osa_crew.empty?
		waitfor /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}: "Crewman #{checkname}, Quarterdeck!\"/
		LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Crewman #{checkname}, Returning For Duty Captain!")
	when /Attention To Quarters!/
		fput "snap attention"
		waitfor /Post!/
		fput "salute #{$osa_commander}" if $osa_commander != checkname
		if (checkname == "Bait") or (checkname == "Subeta") or (checkname == "Ithloria") or (checkname == "Carolanne") or (checkname == "Dargol") or (checkname == "Polyanne")
			fput "exit"
		end
	when /^\[Private\]-GSIV:(.*)\: \"Crewman (.*) Checking Onboard Captain!\"/
		if checkname == $osa_commander
			@newcheckin = $1
			pause 0.5
			LNet.send_message(attr = { 'type' => 'private', 'to' => @newcheckin }, "Excellent Crewman #{@newcheckin}, Welcome Aboard! Our Medical Officer Is: #{$osa_medicalofficer}. Our Shipboard Communications Channel Is: #{$osa_crew}")
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Submit your sever spell requests\!\"/
			pause rand(1..3)
			if $sever_spell.empty?
				nil
			elsif !$sever_spell.empty? && $sever_spell2.empty?
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Require #{}, Please")
			elsif !$sever_spell.empty? && !$sever_spell2.empty? && $sever_spell3.empty?
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Require #{} and #{}, Please")
			elsif !$sever_spell3.empty? && $sever_spell4.empty?
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Require #{}, #{} and #{}, Please")
			else
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Require #{}, #{}, #{} and #{}, Please")
			end
	when /^\[#{$osa_crew}\]-GSIV:(.*): \"I Require (.*), (.*), (.*) and (.*)\, Please/
		person = $1
		sever_spell = $2
		sever_spell2 = $3
		sever_spell3 = $4
		sever_spell4 = $5
		@severlist.push([person, sever_spell, sever_spell2, sever_spell3, sever_spell4])
	when /^\[#{$osa_crew}\]-GSIV:(.*): \"I Require (.*), (.*) and (.*)\, Please/
		person = $1
		sever_spell = $2
		sever_spell2 = $3
		sever_spell3 = $4
		@severlist.push([person, sever_spell, sever_spell2, sever_spell3])
	when /^\[#{$osa_crew}\]-GSIV:(.*): \"I Require (.*) and (.*)\, Please/
		person = $1
		sever_spell = $2
		sever_spell2 = $3
		@severlist.push([person, sever_spell, sever_spell2])
	when /^\[#{$osa_crew}\]-GSIV:(.*): \"I Require (.*)\, Please/
		person = $1
		sever_spell = $2
		@severlist.push([person, sever_spell])
	when /^\[#{$osa_crew}\]-(GSIV:#{$osa_commander}|You)\: \"Does Anyone Need Armor Adjustments\?\"/
		pause rand(1..3)
		if $osa_need_armor_blessing
			if Effects::Spells.time_left("Armor Blessing") <= 120
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Need Armor Blessing, Please")
			end
		end
		if $osa_need_armor_reinforcement
			if Effects::Spells.time_left("Armor Reinforcement") <= 120
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Need Armor Reinforcement, Please")
			end
		end
		if $osa_need_armor_support
			if Effects::Spells.time_left("Armor Support") <= 120
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Need Armor Support, Please")
			end
		end
		if $osa_need_armor_casting
			if Effects::Spells.time_left("Armored Casting") <= 120
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Need Armored Casting, Please")
			end
		end
		if $osa_need_armor_evasion
			if Effects::Spells.time_left("Armored Evasion") <= 120
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Need Armored Evasion, Please")
			end
		end
		if $osa_need_armor_fluidity
			if Effects::Spells.time_left("Armored Fluidity") <= 120
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Need Armored Fluidity, Please")
			end
		end
		if $osa_need_armor_stealth
			if Effects::Spells.time_left("Armored Stealth") <= 120
				LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "I Need Armored Stealth, Please")
			end
		end
	when /^\[#{$osa_crew}\]-GSIV:(.*): \"I Need (Armor|Armored) (.*), Please/
		person = $1
		type = $3
		if type == "Blessing"
			if $osa_have_armor_blessing
				@supportlist.push([person, type])
			end
		elsif type == "Reinforcement"
			if $osa_have_armor_reinforcement
				@supportlist.push([person, type])
			end
		elsif type == "Support"
			if $osa_have_armor_support
				@supportlist.push([person, type])
			end
		elsif type == "Casting"
			if $osa_have_armor_casting
				@supportlist.push([person, type])
			end
		elsif type == "Evasion" 
			if $osa_have_armor_evasion
				@supportlist.push([person, type])
			end
		elsif type == "Fluidity"
			if $osa_have_armor_fluidity
				@supportlist.push([person, type])
			end
		elsif type == "Stealth"
			if $osa_have_armor_stealth
				@supportlist.push([person, type])
			end
		end
	when /You could use this opportunity to (.*)!/
		if $osa_use_reactive
			checkforenemies
			$osa_reactive_creatures.clear
			@npcs.each do |i|
				$osa_reactive_creatures.push (i.id)
			end
			$osa_reactive = true
			$osa_reactivetype = $1
		else
			$osa_reactive = false
		end
	when /^\[Private\]-(Noob|GSIV):(.*)\: \"(heal|Heal|spells|dims|bread|surge KS|surge)\"/
		if $osa_medicalofficer.include? "#{checkname}"
			@person = $2
			echo @person
			pause_script "osacombat"
			if $3 == "heal"
				@medicalofficer_patient.push(@person)
				medicalofficer_checkup
				@medicalofficer_patient.clear
			elsif $3 == "sugre KS"
				medicalofficer_fix_muscles_ks
			elsif $3 == "surge" 
				medicalofficer_fix_muscles
			elsif $3 == "dims"
				medcalofficer_dims
			elsif $3 == "spells"
				medcalofficer_spells
			elsif $3 == "bread" 
				medicalofficer_bread
			end
			unpause_script "osacombat"
		end
	when /^\[#{$osa_crew}\]-GSIV:(.*)\: \"I Am Poisoned!\"$/
		if $osa_medicalofficer.include? "#{checkname}"
			@person = $1
			pause_script "osacombat"
			medicalofficer_fix_poison
			unpause_script "osacombat"
		end
	when /^\[#{$osa_crew}\]-GSIV:(.*)\: \"I Am Diseased!\"$/
		if $osa_medicalofficer.include? "#{checkname}"
			@person = $1
			pause_script "osacombat"
			medicalofficer_fix_disease
			unpause_script "osacombat"
		end
	when /^\[#{$osa_crew}\]-GSIV:(.*)\: \"I Am Injured!\"$/
		if $osa_medicalofficer.include? "#{checkname}"
			pause_script "osacombat"
			@medicalofficer_patient.push($1)
			medicalofficer_triage
			medicalofficer_checkup
			unpause_script "osacombat"
			@medicalofficer_patient.clear
		end
	when /The (Krolvin|Pirate|Ethereal) Captain shouts in rage aboard the (.*) as (he|she) enters the fray!/
		if $osa_cleanup && checkname == $osa_commander
			wait_until {GameObj.targets.count {|npc| npc.status !~ /dead|gone|prone/ && npc.name !~ /animated| arm|arms|tentacle|tentacles/ } < 1}
			start_script "eloot"
			wait_while { running?("eloot") }
			start_script("osacommander", ["cleanup"])
		end
	when /A large swell crashes into the side of the/
		if ($osa_piratehunter)
			pause_script "osacommander"
			waitrt?
			helm
			fput "yell Rogue Wave! Secure the Anchor!"
			waitrt?
			waitcastrt?
			raise_anchor
			fput "turn wheel ship"
			waitrt?
			captains_quarters
			unpause_script "osacommander"
		end
	when /The sound of ropes coming free of the rigging/
		if ($osa_piratehunter)
			pause_script "osacommander"
			waitrt?
			fput "yell The Sails Have Furled, Let Go the Halyard, Sheets, and Braces!"
			waitrt?
			waitcastrt?
			main_deck
			if $osa_ship_type == "sloop" 
				one_mast
			elsif ($osa_ship_type == "brigantine" or $osa_ship_type == "carrack" or $osa_ship_type == "galleon" or $osa_ship_type == "frigate")
				two_masts
			elsif $osa_ship_type == "man o' war"
				three_masts
			end
			fput "turn wheel ship"
			waitrt?
			captains_quarters
			unpause_script "osacommander"
		end
	when /The (.*) suddenly drifts from its course as the/
		if ($osa_piratehunter)
			echo "The Ship Has Gone Off Course"
			pause_script "osacommander"
			waitrt?
			helm
			waitcastrt?
			crew_det_drift
			echo "Corrective Course Determined"
			crew_fix_wheel
			fput "turn wheel ship"
			waitrt?
			captains_quarters
			unpause_script "osacommander"
		end
	when /A distant thudding of the drums of war acts as a harbinger for (.*) ripping|As the water falls\,\ (.*) materializes next to your ship!|(.*)carves through the ocean toward your (.*)/
			if (checkname == $osa_commander) and $osa_piratehunter
				wait_while {GameObj.pcs.any? {|s| s.status =~ /sitting|^lying|prone|stunned/}}
				start_script("osacommander", ['start'])
			end
	when /A sudden booming of cannon fire erupts from the enemy/
			if (checkname == $osa_commander) and ((!$osa_boarding) and $osa_piratehunter)
				wait_while {GameObj.pcs.any? {|s| s.status =~ /sitting|^lying|prone|stunned/}}
				start_script("osacommander", ['start'])
			end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Thirty second warning, drop what yer doing and prepare for battle. Here they come!"/
		@thirty = true
	when /The sides of the (.*) collide against your (.*)/
		@boarded = true
	when /#{$osa_commander}(.*) exclaims, "Send them to the bottom boys!"/
		Lich::Util.quiet_command("target random", /(Could not find a valid target\.|You are now targeting (.*)\.)/, end_pattern = /(Could not find a valid target\.|You are now targeting (.*)\.)/, include_end = false ,timeout = 0.2, silent = true)
	when /^\[Private\]-GSIV:(.*)\: \"Captain\, I\'ve Been Injured and Will Return Shortly\"/
		if checkname == $osa_commander 
			if running? "osacombat"
				pause_script "osacombat"
			end
			if running? "osacombat"
				pause_script "osacommander"
			end
			@crew_being_healed = $1
			LNet.send_message(attr = { 'type' => 'private', 'to' => @crew_being_healed }, "Ok Crewman, We Are At #{Room.current.id} And Will Await Your Arrival")
			wait_until {checkpcs.include?(@crew_being_healed)}
			fput "hold #{@crew_being_healed}"
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"(.*) Is Now Moored In (.*) Room Number: (.*) \[(.*)./
		if !$osa_gangplank.nil? 
			Room[$osa_gangplank].tags.delete("myship") if Room[$osa_gangplank].tags.include? "myship"
		end
		crew_clear_gangplank
		$osa_gangplank = $3
		Room[$osa_gangplank].tags.push("myship") if !Room[$osa_gangplank].tags.include? "myship"
		crew_map_gangplank
	when /^\[Private\]-GSIV:Peggyanne\: \"Update Yo Shit!"/
		if (checkname != "Bait") and (checkname != "Subeta") and (checkname != "Ithloria") and (checkname != "Carolanne") and (checkname != "Polyanne")
			start_script("repository", ["download", "osacrew"])
			wait_while { running?("repository") }
			pause 1
			start_script("repository", ["download", "osacombat"])
			wait_while { running?("repository") }
			pause 1
			start_script("repository", ["download", "osacommander"])
			wait_while { running?("repository") }
			pause 1
			LNet.send_message(attr = { 'type' => 'private', 'to' => 'peggyanne' }, "Update Complete!")
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Update!\"/
		if (checkname != "Bait") and (checkname != "Subeta") and (checkname != "Ithloria") and (checkname != "Carolanne") and (checkname != "Polyanne")
			start_script("repository", ["download", "osacrew"])
			wait_while { running?("repository") }
			pause 1
			start_script("repository", ["download", "osacombat"])
			wait_while { running?("repository") }
			pause 1
			start_script("repository", ["download", "osacommander"])
			wait_while { running?("repository") }
			pause 1
			LNet.send_message(attr = { 'type' => 'channel', 'channel' => $osa_crew }, "Update Complete!")
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Silvers\"/
		crew_give_coins
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Crew, Sell Your Loot!\"/
		if $osa_lootsell
			crew_sell_loot
		else
			pause 5
			crew_task_complete
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"At Ease, Crew!\"/
		wait_until { percentmind < 100 }
		crew_task_complete
	when /^\[Private\]-GSIV:#{$osa_commander}\: \"Your Services Are Requested At (.*) Crewman"/
		start_script("go2", [$1])
		wait_while { running?("go2") }
		waitfor /^\[Private\]-GSIV:#{$osa_commander}\: \"That's All Of Them!"/
		start_script("tpick", ["ground"])
		wait_while { running?("tpick") }
		pause 5
		LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_commander }, "All Set, Captain!")
		captains_quarters
	when /(.*) removes you from (his|her) group./
		$osa_everyone_in_my_group.clear
	when /(.*) (disband|disbands) (his|her|your) group./
		if $osa_everyone_in_my_group.include? $1 or $1 == "You"
			$osa_everyone_in_my_group.clear
		end
	when /(You join (.*).|(.*) gently takes hold of your hand.)/
		determine_group_members
	when /(.*) (remove|removes) (.*) from the group./
		$osa_everyone_in_my_group.delete($3)
		$osa_everyone_in_my_group = $osa_everyone_in_my_group.uniq
	when /(.*) gently (take|takes) hold of (.*)'s hand./
		if $osa_everyone_in_my_group.include? $1 or $1 == "You"
			$osa_everyone_in_my_group.push($3)
		end
		$osa_everyone_in_my_group = $osa_everyone_in_my_group.uniq
	when /You notice your companion ([A-Z]\w*) slip away into hiding./
		$osa_everyone_hidden.push($1)
		$osa_everyone_hidden = $osa_everyone_hidden.uniq
	when /You notice your companion ([A-Z]\w*) emerge from hiding nearby/
		$osa_everyone_hidden.delete($1)
		$osa_everyone_in_my_group.push($1)
		$osa_everyone_in_my_group = ($osa_everyone_in_my_group.uniq - $osa_everyone_hidden.uniq)
		$osa_everyone_hidden =  $osa_everyone_hidden.uniq
	when /(.*) joins your group./
		$osa_everyone_in_my_group.push($1)
		$osa_everyone_in_my_group = $osa_everyone_in_my_group.uniq
	when /You are leading (.*)./
		grouped = $1.to_s.scan(/[A-Z]\w*/).flatten.uniq
		hidden = $1.to_s.scan(/[A-Z]\w* who is hidden/).flatten.uniq
		$osa_everyone_in_my_group.clear
		$osa_everyone_hidden.clear
		grouped.each{|member| $osa_everyone_in_my_group.push(member) if !$osa_everyone_in_my_group.include? (member) }
		hidden.each{|member| $osa_everyone_hidden.push(member.gsub(' who is hidden', '')) if !$osa_everyone_hidden.include? (member) }
		$osa_everyone_in_my_group = ($osa_everyone_in_my_group.uniq - $osa_everyone_hidden.uniq)
		$osa_everyone_hidden =  $osa_everyone_hidden.uniq
	when /You are grouped with (.*)./
		grouped = $1.to_s.scan(/[A-Z]\w*/).flatten.uniq
		hidden = $1.to_s.scan(/[A-Z]\w* who is hidden/).flatten.uniq
		$osa_everyone_in_my_group.clear
		$osa_everyone_hidden.clear
		grouped.each{|member| $osa_everyone_in_my_group.push(member) if !$osa_everyone_in_my_group.include? (member) }
		hidden.each{|member| $osa_everyone_hidden.push(member.gsub(' who is hidden', '')) if !$osa_everyone_hidden.include? (member) }
		$osa_everyone_in_my_group = ($osa_everyone_in_my_group.uniq - $osa_everyone_hidden.uniq)
		$osa_everyone_hidden =  $osa_everyone_hidden.uniq
	when /You leave (.*)'s group./
		if $osa_everyone_in_my_group.include? $1 or $1 == "You"
			$osa_everyone_in_my_group.clear
			$osa_everyone_hidden.clear
		end
	when /You are not currently in a group./
		$osa_everyone_in_my_group.clear
		$osa_everyone_hidden.clear
	when /^(Something stirs in the shadows.|A (.*) darts into the shadows)/
		if $osa_stealth_disabler != 0
			stealth_disabler_routine
			fput "target random"
		end
	when /^\[(#{$osa_commander}|Private)\]-GSIV:#{$osa_commander}\: \"Command\: (.*)\"/
		$command = $2
		if (checkname == "Bait") or (checkname == "Subeta") or (checkname == "Ithloria") or (checkname == "Carolanne") or (checkname == "Polyanne")
			do_client $command
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Command\: (.*)\"/
		$command = $1
		if (checkname == "Bait") or (checkname == "Subeta") or (checkname == "Ithloria") or (checkname == "Carolanne") or (checkname == "Polyanne")
			do_client "#{$command}"
		end
	when /^A collection of dock workers can be heard shouting from nearby/
		if (checkname == $osa_commander) && $osa_anti_stow && !$osa_no_stow
			start_script("osacommander", ["anti_stow"])
		end
	when /obvious signs of someone hiding/
		#echo "changing value to true!"
		$osa_pc_hiding = true
		$osa_pc_hiding_room = Room.current.id
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Steel Yourself, (.*)!\"/
		if checkname == $1
			if !running? "osacombat"
				start_script "osacombat"
				pause rand(1..8)
				LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_commander }, "Ready For Combat Captain!")
			end
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Enable Poaching!\"/
		if $osa_check_for_group != true
			$save_new_data = Hash.new
			$save_new_data.store(:$osa_check_for_group, true)
			self.save_script_data
			respond ""
			respond "-----------------------***|   Poaching Has Been Enabled   |***----------------------------"
			respond ""
		else
			respond ""
			respond "-----------------------***|   Poaching Is Already Enabled   |***----------------------------"
			respond ""
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Disable Poaching!\"/
		if $osa_check_for_group == true
			$save_new_data = Hash.new
			$save_new_data.store(:$osa_check_for_group, false)
			self.save_script_data
			respond ""
			respond "-----------------------***|   Poaching Has Been Disabled   |***----------------------------"
			respond ""
		else
			respond ""
			respond "-----------------------***|   Poaching Is Already Disabled   |***----------------------------"
			respond ""
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Loot the Dead, (.*)!\"/
		if checkname == $1
			$osa_osalooter = true
			respond ""
			respond "Switching to Primary Party Looter"
			respond ""
		elsif $1 == "Crew"
			$osa_osalooter = true
			respond ""
			respond "Switching On Loot The Dead"
			respond ""
		else
			$osa_osalooter = false
		end
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Steel Yourselves Crew!\"/
		if !running? "osacombat"
			start_script "osacombat"
			pause rand(1..8)
			LNet.send_message(attr = { 'type' => 'private', 'to' => $osa_commander }, "Ready For Combat Captain!")
		end
	when /^A virulent green mist seeps out of (.*) lips toward you,/
		$osa_healing_status = "diseased"
	when /^Your (.*) tears free from your hands and floats threateningly in the air around you, darting about as if possessed!/
		$osa_myweapon = $1.split.last.delete('.!?,')
	when /^\[#{$osa_crew}\]-GSIV:#{$osa_commander}\: \"Spell Up Completed"/
		if Feat.known?("Kroderine Soul")
			pause 0.5
			fput "join #{$osa_commander}"
		end
	end
end