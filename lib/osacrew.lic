def self.add_dropdown(table, dropdown, tooltip, variable, options, first_alignment, first_alignment2, second_aligntment, second_aligntment2, newrow = false)
	size = @OSA_CREW_TABLE_SIZE[table] || 0
	label = Gtk::Label.new(dropdown)
	label.set_tooltip_text(tooltip)
	yamlpath = Settings[Char.name]['yamlpath'] || File.join(DATA_DIR, XMLData.game, XMLData.name)
	$osa_vars_yamlname = "osa_vars.yaml"
	$osa_vars_yamlpath = "#{yamlpath}"
    full_path = File.join($osa_vars_yamlpath, $osa_vars_yamlname)
	$yaml_data = YAML.load_file(full_path, permitted_classes: [Time, Symbol])
	dropdown = 
	if Gtk.check_version(3,0,0).nil?
		Gtk::ComboBoxText.new
	else
		Gtk::ComboBox.new
	end
	options.split(',').each { |s| dropdown.append_text(s) }
	if $yaml_data[variable].nil?
		dropdown.set_active(0)
	else
		if $yaml_data[variable].to_s != "Fixnum" && $yaml_data[variable].to_s != "Integer"
			$yaml_data[variable]
		end
		dropdown.set_active($yaml_data[variable])
	end
	align = Gtk::Alignment.new 0, 0, 0, 0
	align2 = Gtk::Alignment.new 0, 0, 1, 0
	align.set_padding(4, 0, 3, 4)
	align2.set_padding(0, 0, 0, 0)
	align.add(label)
	align2.add(dropdown)
	table.attach(align, first_alignment, first_alignment2, size, size + 1)
	table.attach(align2, second_aligntment, second_aligntment2, size, size + 1)
	@OSA_CREW_ENTRY[variable] = dropdown
	@OSA_CREW_TABLE_SIZE[table] += 1 if newrow
end

def self.add_text_area(vbox, label, variable)
	current_value = $yaml_data[variable]
	entry = Gtk::TextView.new
	entry.buffer.text = current_value ? current_value.gsub("||","\n") : ""
	swin = Gtk::ScrolledWindow.new
	swin.border_width = 5
	swin.add(entry)
	swin.set_policy(Gtk::POLICY_AUTOMATIC, Gtk::POLICY_ALWAYS)
	frame = Gtk::Frame.new(label)
	frame.add(swin)
	vbox.pack_start_defaults(frame)
	@OSA_CREW_ENTRY[variable] = entry
end
	
