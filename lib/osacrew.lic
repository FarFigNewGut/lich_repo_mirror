	if @winded
		pause 5
		return
	else
		main_deck
		return
	end
end

def winded_sails
	if @winded
		main_deck
		cast_sails
	else
		main_deck
		return
	end
	if @winded
		if $osa_ship_map.include? "mid_deck"
			mid_deck
			cast_sails
		end
	else
		main_deck
		return
	end
	if @winded
		if $osa_ship_map.include? "forward_deck"
			forward_deck
			cast_sails
		end
	else
		main_deck
		return
	end
	10.times{check_winded}
	winded_sails
end

