                echo "Targetting someone else." if debug
                echo "Target active." if debug
            else
                echo "Target lost." if debug
            end
        end
    else
        echo "An error has occured while setting a target."
    end
end

input = script.vars[1].downcase

if script.vars.empty? == false
    if verb_hash[input]
        echo "Matching verb found." if debug    
        display = ""        
        if verb_hash[input].styles.nil? || verb_hash[input].styles.empty?
            if verb_hash[input].condition == 'self'
                target = Char.name
            end
            echo "Executing verb: #{verb_hash[input].verb}" if debug
            execute_verb(verb_hash[input].verb, target)
            exit
        elsif verb_hash[input].styles
            i = 1
            display << verb_hash[input].id + ".#{i}" + ": " + verb_hash[input].verb
            if verb_hash[input].allowstarget == true && verb_hash[input].requirestarget == false
                display << " [<target>]"
            elsif verb_hash[input].allowstarget == true && verb_hash[input].requirestarget == true
                display << " <target>"
            elsif verb_hash[input].allowstarget == false && verb_hash[input].requirestarget == false
                display << " (providing a target won't change the behavior)"
            else
                echo "There was an error processing your input."
            end
            display << "\n"
            verb_hash[input].styles.each { |style|
                i += 1
                display << verb_hash[input].id + ".#{i}" +": " + verb_hash[input].verb + " " + style
                if verb_hash[input].allowstarget == true && verb_hash[input].requirestarget == false
                    display << " [<target>]"
                elsif verb_hash[input].allowstarget == true && verb_hash[input].requirestarget == true
                    display << " <target>"
                elsif verb_hash[input].allowstarget == false && verb_hash[input].requirestarget == false
                    display << " (providing a target won't change the behavior)"
                else
                    echo "There was an error processing your input."
                end
                display << "\n"
            }
            respond
            respond display
            respond
            exit
        else
            echo "There was an error processing your input."
        end
    else
        echo "No directly matching verb found." if debug
    end

    input_array = input.chars
    input_prefix = ""
    input_suffix = ""
    input_array.each { |digit|
        if digit == "."
            break
        else
            input_prefix << digit
        end
    }
    digit_number = 1
    input_array.each { |digit|
        if digit == "."
            if input_array[digit_number]
                input_suffix << input_array[digit_number]
            end
            if input_array[digit_number + 1]
                input_suffix << input_array[digit_number + 1]
            end
        end
        echo "Parsing input digit #{digit_number}: \"#{digit}\"" if debug
        digit_number += 1        
    }

    echo "Parsed suffix: #{input_suffix}" if debug

    input_suffix = input_suffix.to_i
    echo "Correct variable type for verb execution: #{input_prefix} #{input_prefix.is_a?(String)}" if debug
    echo "Correct variable type for style execution: #{input_suffix} #{input_suffix.is_a?(Integer)}" if debug
  
    if Verb.id_list.include? input_prefix
        echo "Number of styles found: #{verb_hash[input_prefix].styles.length}" if debug
        Verb.id_list.each { |verb_id|
            echo "Comparing verb with ID for execution: #{verb_id}" if debug
            if input_prefix == verb_id
                echo "Verb ID found for execution: #{verb_id}" if debug
                if input_suffix == 1
                    echo "Executing verb without style: #{verb_hash[input_prefix].verb}" if debug
                    execute_verb(verb_hash[input_prefix].verb, target)
                    break
                elsif input_suffix > 1 && verb_hash[input_prefix].styles.length + 1 >= input_suffix
                    echo "Executing verb: #{verb_hash[input_prefix].verb}" if debug
                    echo "Executing verb style: #{verb_hash[input_prefix].styles[input_suffix - 2]}" if debug
                    echo "Executing target: #{target}" if debug
                    execute_styled_verb(verb_hash[input_prefix].verb, verb_hash[input_prefix].styles[input_suffix - 2], target)
                    break
                else
                    echo "There was an error executing a verb."
                end
            end
        }
    end
end

