debug = false
target = nil


def execute_verb(verb_input, target_input)
    if target_input
        echo "Executing targeted verb." if debug
        fput "#{verb_input} #{target_input}"
    elsif target_input == false || target_input == nil
        echo "Executing not targeted verb." if debug
        fput "#{verb_input}"
    else
        echo "There was an error executing a verb."
    end
end

def execute_styled_verb(verb_input, style_input, target_input)
    if target_input
        echo "Executing targeted verb." if debug        
        fput "#{verb_input} #{style_input} #{target_input}"
    elsif target_input == false || target_input == nil
        echo "Executing not targeted verb." if debug
        fput "#{verb_input} #{style_input}"
    else
        echo "There was an error executing a verb."
    end
end

class Verb
    @@verb_array = Array.new

    attr_accessor :id, :verb, :styles, :allowstarget, :requirestarget, :requiresstyle, :requiresstyleortarget, :modifier, :condition, :allowsracespecific, :requiresrace, :effect, :category

    def Verb.id_list
        @@verb_array
    end
        
    def Verb.check(input_id)
        found_count = 0
        @@verb_array.each { |id|
            if id == input_id
                found_count += 1
            end
        }
        if found_count == 1
            return true
        else
            return false
        end
    end

    def initialize(args = {})
        @id = args[:id]
        @verb = args[:verb]
        @styles = args[:styles]
        @allowstarget = args[:allowstarget]
        @requirestarget = args[:requirestarget]
		@requiresstyle = args[:requiresstyle]
		@requiresstyleortarget = args[:requiresstyleortarget]		
        @modifier = args[:modifier]
        @condition = args[:condition]
        @allowsracespecific = args[:allowsracespecific]
        @requiresrace = args[:requiresrace]
        @effect = args[:effect]
        @category = args[:category]
        @@verb_array.push(@id)
    end
    
end

