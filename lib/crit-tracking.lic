		if current_enemy_armor =~ /#{key}/
			chest_crit_divisor = all_armor_divisor_info[key][:chest_divisor]
			abdomen_crit_divisor = all_armor_divisor_info[key][:abdomen_divisor]
			back_crit_divisor = all_armor_divisor_info[key][:back_divisor]
			arm_crit_divisor = all_armor_divisor_info[key][:arm_divisor]
			hand_crit_divisor = all_armor_divisor_info[key][:hand_divisor]
			leg_crit_divisor = all_armor_divisor_info[key][:leg_divisor]
