do_critter_gear_stuff = proc{
	if UserVars.crittracking
		if critter_weapon
			critter_weapon_message = "Weapon:                #{critter_weapon}"
		else
			critter_weapon_message = "Weapon:                None"
		end
		if critter_shield
			critter_shield_message = "Shield: #{critter_shield}"
		else
			critter_shield_message = "Shield: None"
		end
		if critter_worn_armor
			critter_torso_armor_message = "Worn Torso Armor: #{critter_worn_armor}"
		else
			critter_torso_armor_message = "Worn Torso Armor: None"
		end
		if critter_worn_arm_hand_protection
			critter_arm_message = "Arm & Hand Protection: #{critter_worn_arm_hand_protection}"
		else
			critter_arm_message = "Arm & Hand Protection: None"
		end
		if critter_worn_leg_protection
			critter_leg_message = "Leg Protection: #{critter_worn_leg_protection}"
		else
			critter_leg_message = "Leg Protection: None"
		end
		if critter_worn_head_protection
			critter_head_message = "Head Protection: #{critter_worn_head_protection}"
		else
			critter_head_message = "Head Protection: None"
		end
		if critter_worn_eye_protection
			critter_eye_message = "Eye Protection: #{critter_worn_eye_protection}"
		else
			critter_eye_message = "Eye Protection: None"
		end
		if critter_worn_neck_protection
			critter_neck_message = "Neck Protection: #{critter_worn_neck_protection}"
		else
			critter_neck_message = "Neck Protection: None"
		end
	else
		if critter_weapon
			critter_weapon_message = "Weapon:                #{critter_weapon}"
		else
			critter_weapon_message = "Weapon:                None"
		end
		if critter_shield
			critter_shield_message = "Shield:                #{critter_shield}"
		else
			critter_shield_message = "Shield:                None"
		end
		if critter_worn_armor
			critter_torso_armor_message = "Worn Torso Armor:      #{critter_worn_armor}"
		else
			critter_torso_armor_message = "Worn Torso Armor:      None"
		end
		if critter_worn_arm_hand_protection
			critter_arm_message = "Arm & Hand Protection: #{critter_worn_arm_hand_protection}"
		else
			critter_arm_message = "Arm & Hand Protection: None"
		end
		if critter_worn_leg_protection
			critter_leg_message = "Leg Protection:        #{critter_worn_leg_protection}"
		else
			critter_leg_message = "Leg Protection:        None"
		end
		if critter_worn_head_protection
			critter_head_message = "Head Protection:       #{critter_worn_head_protection}"
		else
			critter_head_message = "Head Protection:       None"
		end
		if critter_worn_eye_protection
			critter_eye_message = "Eye Protection:        #{critter_worn_eye_protection}"
		else
			critter_eye_message = "Eye Protection:        None"
		end
		if critter_worn_neck_protection
			critter_neck_message = "Neck Protection:       #{critter_worn_neck_protection}"
		else
			critter_neck_message = "Neck Protection:       None"
		end
	end
	all_temps.each{ |i|
		if i
			critter_worn_all_gear.push(i, )
			critter_has_gear = true
		end
	}
	if critter_has_gear
		all_critter_gear_message = "All Critter Gear:      #{critter_worn_all_gear}"
	else
		all_critter_gear_message = "All Critter Gear:      None"
	end
}

