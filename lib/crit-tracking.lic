while (line = get)
	if (line =~ /You (swing|fire|hurl|thrust|slash|throw|punch|attempt to punch|attempt to kick|attempt to throw).* an? (.*) at .* \<a exist\=\"(.*)\" noun\=\".*\"\>(.*)\<\/a\>.*\!/)
		nil_everything_out.call
		weapon_name = $2 unless weapon_name
		enemy_id = $3 unless enemy_id
		enemy_name = $4 unless enemy_name
		if line =~ /You fire/
			using_ranged_weapon = true
		end
		if full_weapon_name == nil
			if variable_inputted_weapon_name
				full_weapon_name = variable_inputted_weapon_name
			elsif using_ranged_weapon
				full_weapon_name = GameObj.left_hand.name
			elsif GameObj.right_hand.name == "Empty"
				full_weapon_name = "closed fist"
			else
				full_weapon_name = GameObj.right_hand.name
			end
		end
		if weapon_name =~ /\bfine\b/
			forging_bonus = "fine" unless forging_bonus
		elsif weapon_name =~ /\belegant\b/
			forging_bonus = "elegant" unless forging_bonus
		elsif weapon_name =~ /\bwell\-made\b/
			forging_bonus = "well-made" unless forging_bonus
		elsif weapon_name =~ /\bexquisite\b/
			forging_bonus = "exquisite" unless forging_bonus
		elsif weapon_name =~ /\bwell\-crafted\b/
			forging_bonus = "well-crafted" unless forging_bonus
		elsif weapon_name =~ /\bnifty\b/
			forging_bonus = "nifty" unless forging_bonus
		elsif weapon_name =~ /\bhefty\b/
			forging_bonus = "hefty" unless forging_bonus
		elsif weapon_name =~ /\bsuperior\b/
			forging_bonus = "superior" unless forging_bonus
		elsif weapon_name =~ /\bperfect\b/
			forging_bonus = "perfect" unless forging_bonus
		else
			forging_bonus = "none" unless forging_bonus
		end
		while (line = get)
			if (line =~ /\s+AS\: [\+|\-]\d+ vs DS\: [\+|\-]\d+ with AvD\: [\+|\-](\d+) \+ d100 roll\: \+\d+ \= \+(\d+)/)
				avd = ($1.to_i) unless avd
				end_roll = ($2.to_i) unless end_roll
				adjust_forging_bonus_numbers.call
				if Char.prof == 'Paladin' && Spell[1605].active?
					bonus = 0
					[5,11,18,26,35,45,56,68,81,95,110,126,143,161,180,200].each { |n| bonus += 1 if Skills.slsummoning >= n }
					bonus_1605 = (100.0 + (10 + bonus)) / 100
					if UserVars.crittracking
						bonus_1605_message = "1605 Bonus: #{bonus_1605}%"
					else
						bonus_1605_message = "1605 Bonus:    #{bonus_1605}%"
					end
				else
					bonus_1605 = 1
					if UserVars.crittracking
						bonus_1605_message = "1605 Bonus: None"
					else
						bonus_1605_message = "1605 Bonus:    None"
					end
				end
				if 101 > end_roll
					break
				else
					while (line = get)
						crit_table.each { |key,value|
							if line =~ /#{key}/
								crit_rank = crit_table[key][:r] unless crit_rank
								crit_bonus_damage = crit_table[key][:d] unless crit_bonus_damage
								crit_location = crit_table[key][:l] unless crit_location
								crit_type = crit_table[key][:t] unless crit_type
								status_effect = crit_table[key][:stat] unless status_effect
								wound_effect = crit_table[key][:wound] unless wound_effect
								crit_message = line unless crit_message
								if crit_message =~ /(.*)\<pushBold\/\>(.*)\<a exist\=\".*\" noun\=\".*\"\>(.*)\<\/a\>\<popBold\/\>(.*)/
									crit_message_a = $1
									crit_message_b = $2
									crit_message_c = $3
									crit_message_d = $4
									crit_message = "#{crit_message_a}#{crit_message_b}#{crit_message_c}#{crit_message_d}"
								end
							end
						}
						if (line =~ /(\d+) points? of damage\!/)
							total_damage = $1.to_i unless total_damage
						elsif (line =~ /Roundtime\:/)
							break
						end
					end
					weapon_type_list.each { |key,value|
						if (full_weapon_name =~ /#{key}/i) && (avd == weapon_type_list[key][:avd])
							current_enemy_armor = weapon_type_list[key][:armor_type]
							gather_critter_information.call unless critter_info_gathered
							critter_info_gathered = true
							current_df = weapon_type_list[key][:df]
							fix_asg_numbers.call
							current_weapon_type = weapon_type_list[key][:wt]
							base_raw_damage = sprintf('%.3f', (end_roll - 100) * (current_df))
							if forging_df_bonus == 1
								forging_raw_damage = 0
							else
								forging_raw_damage = sprintf('%.3f', (end_roll - 100) * (((current_df) * (forging_df_bonus)) - (current_df)))
							end
							if bonus_1605 == 1
								bonus_1605_raw_damage = 0
							else
