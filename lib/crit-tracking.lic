			arm_crit_divisor = chest_crit_divisor if critter_worn_arm_hand_protection
			hand_crit_divisor = chest_crit_divisor if critter_worn_arm_hand_protection
			leg_crit_divisor = chest_crit_divisor if critter_worn_leg_protection
			head_crit_divisor = chest_crit_divisor if critter_worn_head_protection
			eye_crit_divisor = chest_crit_divisor if critter_worn_eye_protection
			neck_crit_divisor = chest_crit_divisor if critter_worn_neck_protection
		end
	}
	if crit_location =~ /chest/i
		final_crit_divisor = chest_crit_divisor
	elsif crit_location =~ /abdomen/i
		final_crit_divisor = abdomen_crit_divisor
	elsif crit_location =~ /back/i
		final_crit_divisor = back_crit_divisor
	elsif crit_location =~ /arm/i
