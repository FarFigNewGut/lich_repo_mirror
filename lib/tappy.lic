=begin

	For using 909 tap in bigshot
	Author:Venzo
	
	
=end

if !checkmana(5)
	echo "Not enough manas!"
	exit
end
result = fput "tap"
if result =~ /impatiently/
	if Spell[909].affordable?
		fput "stanc def"
		Script.pause("bigshot")
		waitcastrt?
		multifput "prep 909","evoke"
		if checkmana(5)
			waitcastrt?
			fput "tap"
		end
		Script.unpause("bigshot")
	end
		
end