  def feat_control
    if $huntpro_feat_shadowdance == "1"
      waitrt?
      fput "feat shadowdance" if percentstamina >= 50
      waitrt?
    end

    if $huntpro_feat_silentstrike == "1"
      waitrt?
      Huntpro_toolkit.hide_time
      Huntpro_toolkit.stance_offensive
      fput "feat silentstrike target" if percentstamina >= 50
      waitrt?
      Huntpro_toolkit.hide_time
      Huntpro_toolkit.stance_offensive
      fput "feat silentstrike target" if percentstamina >= 50
      waitrt?
    end

    if $huntpro_feat_mysticstrike == "1"
      waitrt?
      fput "feat mysticstrike target" if percentstamina >= 50
      waitrt?
    end

    if $huntpro_feat_dispelmagic == "1"

      fput "feat dispel" if $huntpro_feat_dispelmagic_cooldown == 0
      $huntpro_feat_dispelmagic_cooldown = 10 if $huntpro_feat_dispelmagic_cooldown == 0
    end

    if $huntpro_feat_absorbmagic == "1"
      fput "feat absorb" if $huntpro_feat_absorbmagic_cooldown == 0
      $huntpro_feat_absorbmagic_cooldown = 30
    end

    $huntpro_feat_absorbmagic_cooldown = $huntpro_feat_absorbmagic_cooldown - 1 unless $huntpro_feat_absorbmagic_cooldown == 0
    $huntpro_feat_dispelmagic_cooldown = $huntpro_feat_dispelmagic_cooldown - 1 unless $huntpro_feat_dispelmagic_cooldown == 0
  end

