	def script_kill
	
		Huntpro_toolkit.stow_weapon
		Script.kill "intel_huntpro"
		Script.kill "huntpro"
	end
	
	def loot_script_sell(scriptname)
	
		do_client (";#{scriptname} sell")
		wait_while {running? ";#{scriptname}"}
		Huntpro_toolkit.wait_loot
	
	end
	
	
	def loot_script(scriptname)
		waitrt?
		if Script.running? "#{$huntpro_loot_script}"
			#respond "Debug Output - Loot Script is Running."
		else
			if GameObj.dead.length >= 1
				do_client ";#{scriptname}"
				wait_while {running? ";#{scriptname}"}
			end
		end
	end
	
	def wait_loot
	
		wait_while {running? "#{$huntpro_loot_script}"}	
		
	end
	
	def cloud_check
		GameObj.loot.each { |i|
		if i.noun =~ /cloud/
			walk
			Huntpro_toolkit.evicted
			sleep 0.025
		end	
		}
	end
	
	def void_check
		GameObj.loot.each { |i|
