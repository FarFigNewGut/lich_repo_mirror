		$huntpro_current_id_weak_targets = nil
		$huntpro_current_id_weak_targets = Array.new
		
		$huntpro_current_name_weak_targets = nil
		$huntpro_current_name_weak_targets = Array.new

		$huntpro_current_name_targets = nil
