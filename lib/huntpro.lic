huntpro_ready_gear_check.call if huntpro_jfloo_ready_weapon == 1

if huntpro_setting_display_xnarost.active?
	do_client ";xnarost" unless Script.running? "xnarost"
end



loop {

huntpro_zone_reset_check.call
if huntpro_zone_reset == 1
	huntpro_zone_reset = 0
	do_client ";go #{huntpro_zone_goto}"
	wait_while {running? "go2"}	
end

if Script.running? "intel_huntpro"
	nil
else
	do_client ";intel_huntpro"
end

huntpro_status_check.call
huntpro_upkeep_buffs.call
huntpro_upkeep_society.call
huntpro_status_check.call
huntpro_gameobj_check.call
intel_huntpro_fail_safe.call

	if $huntpro_action == 0
		#respond "Debug Output - Making a Tier Decision 0."
		Huntpro_toolkit.tier_decision
	
	elsif $huntpro_action == 1
		#respond "Debug Output - Tier 1 - Hard Exit."
		Huntpro_toolkit.tier_1_exit
	
	elsif $huntpro_action == 2
		#respond "Debug Output - Tier 2 - Movement & Loot Only"
		$huntpro_action = 5
	
	elsif $huntpro_action == 3
		#respond "Debug Output - Tier 3 - Full Combat"
		$huntpro_action = 5
	
	elsif $huntpro_action == 4
		#respond "Debug Output - Quick Combat."
		$intel_huntpro_watch = 1
		Huntpro_toolkit.quick_combat
		$intel_huntpro_watch = 0
	
	elsif $huntpro_action == 5
		#respond "Debug Output - This is where I'd handle groups."
		#Rework PCS when group mode launches
		#Work in Group Mode Here
		#huntpro_pcs_snapshot.call
		$huntpro_action = 6
	
	elsif $huntpro_action == 6
		#Work in Own Room
		#respond "Debug Output - This is where I'd move + Own Room."
		$intel_huntpro_watch = 0
		Huntpro_toolkit.movemgr
		
	elsif $huntpro_action == 7
		#respond "Debug Output - Decision Fork Outpost"
		#respond "Debug Output - T2 = Loot  T3 = Combat"
		if $huntpro_my_tier == 2
			Huntpro_toolkit.tier_2_loot
		elsif $huntpro_my_tier == 3
			#respond "Debug Output - This is where I'd do combat."
			$intel_huntpro_watch = 1
			Huntpro_toolkit.combat
		end		
		
	elsif $huntpro_action == 97
		#respond "Debug Output - This is where I'd recover a weapon."
		if $huntpro_ignore_disarm == 1
			$recover_line = 0
			$huntpro_action = 0
		else
			Huntpro_toolkit.recover_item
			$recover_line = 0
			$huntpro_action = 0
		end
	elsif $huntpro_action == 98
		#respond "Debug Output - Walk & Reset."
		Huntpro_toolkit.room_walk		
		
	elsif $huntpro_action == 99
		#respond "Debug Output - Returning to safe room."
		if $safe_quick_trip == 1
			Huntpro_toolkit.loot_script("#{$huntpro_loot_script}")
			Huntpro_toolkit.wait_loot
			Huntpro_toolkit.top_menu
			respond "I aborted quick combat because: #{$huntpro_return_why}"
			Huntpro_toolkit.bottom_menu
			Huntpro_toolkit.script_kill			
		else
			Huntpro_toolkit.safe_wrap_up		
		end
	end


#respond "Debug Output - 1 Line through Huntpro"

}