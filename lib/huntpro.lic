if script.vars[1].downcase =~ /preface/
	Huntpro_toolkit.top_menu
	Huntpro_toolkit.preface
	Huntpro_toolkit.bottom_menu
	Huntpro_toolkit.script_kill	
end

#Discord Check
if script.vars[1].downcase =~ /discord/
	Huntpro_toolkit.top_menu
	respond "Hi #{Char.name}! Please join us on our Discord platform."
	respond
	respond "Huntpro Feedback: https://discord.gg/Cdyrjje - (Project improves with user suggestions.)"
	Huntpro_toolkit.bottom_menu
	exit
end

#Support Check
if script.vars[1].downcase =~ /support/
	Huntpro_toolkit.top_menu
	respond "Hi #{Char.name}! Based on user feedback, the project now has support options. "
	respond
	respond "Option 1: Gift of Adventure - https://www.play.net/gs4/goa - Account - Jara"
	respond
	respond "Option 2: Patreon - https://www.patreon.com/GSIVJara "
	respond
	respond "Option 3: Paypal - jara@play.net "
	respond
	respond "Donating is never required but thank you for your support if you so choose!"
	Huntpro_toolkit.bottom_menu
	exit
end

#Laundry Check
if script.vars[1].downcase =~ /laundry/
	Huntpro_toolkit.top_menu
	huntpro_laundry.call
	Huntpro_toolkit.bottom_menu
	exit
end

#Sell Check
if script.vars[1].downcase =~ /sell/
	Huntpro_toolkit.top_menu
	respond "Hi #{Char.name}, I will now complete housekeeping."
	respond 
	respond "Current Task List: "
	respond
	respond "Lootscript Sell "
	Huntpro_toolkit.bottom_menu
	huntpro_empty_hands.call
	$huntpro_loot_script = "#{huntpro_setting_loot_script}"
	Huntpro_toolkit.loot_script_sell("#{$huntpro_loot_script}")
	if huntpro_setting_sell_return_resting_room.active?
		$huntpro_resting_room = "#{huntpro_setting_resting_room}"
		Huntpro_toolkit.sell_safe_room
	end
end

#Huntpro Setup Effects begin here.

