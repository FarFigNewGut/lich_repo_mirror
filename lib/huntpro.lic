					Settings[Char.name]['huntpro_setting_equipment_sack_gem']	= "#{Vars.boxsack}"
					Settings[Char.name]['huntpro_setting_equipment_sack_skin']	= "#{Vars.boxsack}"
					Settings[Char.name]['huntpro_setting_equipment_sack_herb']	= "#{Vars.boxsack}"
					Settings.save
				end
			end
			
			if huntpro_setting_equipment_sack_default != "0"
				nil
