	def reim_entry_new_user
		#New User Reim Experience
		if Room.current.id == 9147
			fput "out"
		end
		
		Script.run('go2', '24876')
		pause 1
		result = dothistimeout "climb chain", 2, /feel that you will have to wait another|rebukes your advances|jumping out towards the massive chains|Are you sure you wish to enter/
					if result =~ /rebukes your advances/
						Huntpro_toolkit.top_menu
						respond "Hi #{Char.name}, you don't have access to Reim."
						respond
						respond "Please purchase access and try again, thank you!"
						Huntpro_toolkit.bottom_menu
						Huntpro_toolkit.script_kill
						
					elsif result =~ /feel that you will have to wait another/
						Huntpro_toolkit.top_menu
						respond "Hi #{Char.name}, you must wait longer to access Reim."
						respond
						respond "Please try back later, thank you!"
						Huntpro_toolkit.bottom_menu
						Huntpro_toolkit.script_kill
						
					elsif result =~ /Are you sure you wish to enter/
						fput "climb chain"
