	def stance_dance
		
	
		if $huntpro_style_3_stance_dance == 1
			#respond "Debug Output - Now in stance dance"
			Huntpro_toolkit.stance_guarded
			
			loop {
			#respond "Debug Output - My retaliate setting is #{$huntpro_retaliate}"
			if $huntpro_retaliate == 1
				break
			end
			
			if GameObj.targets.length == 0
				break
			end
			
			if percenthealth <= 50
				break
			end
			
			sleep 0.25
			#respond "Debug Output - Now waiting to be attacked."
			
			}
			
			$huntpro_retaliate = 0
		end	
	end
	
