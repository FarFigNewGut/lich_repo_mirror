	def stow_all
		
		if $huntpro_right_hand_detect == "0" && $huntpro_left_hand_detect == "0"
			
			if checkright == nil && checkleft == nil
				nil
			else
				fput "sheath"
				#fput "store weapon"
				fput "store ranged" if $huntpro_my_style =~ /5|6|7|8/
				fput "store shield" if Skills.shielduse >= 1
				fput "stow all"			
			end
		else
			nil			
		end
		
		if $huntpro_right_hand_detect != "0" && $huntpro_left_hand_detect == "0"
			if checkleft != nil
				fput "store ranged" if $huntpro_my_style =~ /5|6|7|8/
				fput "store shield" if Skills.shielduse >= 1
				fput "stow left"
			end
			
			if checkright == "#{$huntpro_right_hand_detect}"
				nil
			else
				fput "sheath"
				#fput "store weapon"
				fput "stow right"
			end
		end
		
		
		if $huntpro_right_hand_detect == "0" && $huntpro_left_hand_detect != "0"
			if checkright != nil
				fput "sheath"
				#fput "store weapon"
				fput "stow right"
			end
			
			if checkleft == "#{$huntpro_left_hand_detect}"
				nil
			else
				fput "store ranged" if $huntpro_my_style =~ /5|6|7|8/
				fput "store shield" if Skills.shielduse >= 1
				fput "stow right"
			end
		
		end
		
		if $huntpro_action == 99
			fput "sheath"
			#fput "store weapon"
			fput "store ranged" if $huntpro_my_style =~ /5|6|7|8/
			fput "store shield" if Skills.shielduse >= 1
			fput "stow all"
		end
		
		
		
		
	end
	
