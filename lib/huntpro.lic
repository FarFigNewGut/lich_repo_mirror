	def stow_weapon
		
		if checkleft == nil && checkright == nil
			nil
		else
			if $huntpro_auto_stow == 1
				#respond "Debug Output - Auto Stowing"
				if $huntpro_style_3_dual_wield == 1
					fput "sheath"
					fput "sheath"
				else
					if Char.name == "Gherraltis"
						fput "tap my ono"
					end
					fput "stow all"
				end
			end
		end
	
	end
	
	def unpause_go2
		sleep 3
		do_client ";ua"
		if Script.running? "go2"
			wait_while {running? "go2"}
		else
			nil
		end
	end
	
	def reim_room_fix
		if Room.current.id == 24965
			waitrt?
			fput "climb rope"
		end
		
		if Room.current.id == 24964
			fput "west"
		elsif Room.current.id == 24971
			fput "west"
			fput "west"
		elsif Room.current.id == 24972
			fput "south"
			fput "west"
			fput "west"			
		end
	end
	
	def safe_room_custom(roomnumber)
		Huntpro_toolkit.loot_script("#{$huntpro_loot_script}")
		Huntpro_toolkit.wait_loot
		do_client ";go #{roomnumber}"
		Huntpro_toolkit.unpause_go2
		Huntpro_toolkit.top_menu
		respond "I am at your preferred resting room because: #{$huntpro_return_why}"
		Huntpro_toolkit.bottom_menu
		Huntpro_toolkit.script_kill
	end
	
