  def dispose_wand
    # respond "DEBUG put wand in dead container"
    # Put wand in deadwand container
    if $huntpro_dead_wands == "0"
      if $huntpro_wand_warning == "0"
        Huntpro_toolkit.top_menu
        respond "#{Char.name}, you do not have a dead wand container set."
        respond
        respond "I'll start dropping all non combat or dead wands."
        respond
        respond "I suggest you ;ka your scripts and set your deadwands container."
        respond
        respond ";huntpro setup change deadwands pack"
        respond
        respond "Pick a container that is not your lootsack."
        Huntpro_toolkit.bottom_menu
        $huntpro_wand_warning = "1"
        sleep 2
      end
      if checkright == "#{$huntpro_current_wand_noun}"
        fput "drop right"
        $huntpro_current_wand_name = "0"
        $huntpro_recent_dead_wand = "1"
      elsif checkleft == "#{$huntpro_current_wand_noun}"
        fput "drop left"
        $huntpro_current_wand_name = "0"
        $huntpro_recent_dead_wand = "1"
      end
    else
      # Dead Wand Container Is set
      # respond "DEBUG wand container is set"
      if checkright == "#{$huntpro_current_wand_noun}"
        fput "put my #{$huntpro_current_wand_noun} in my #{$huntpro_dead_wands}"
        $huntpro_current_wand_name = "0"
        $huntpro_recent_dead_wand = "1"
      elsif checkleft == "#{$huntpro_current_wand_noun}"
        fput "put my #{$huntpro_current_wand_noun} in my #{$huntpro_dead_wands}"
        $huntpro_current_wand_name = "0"
        $huntpro_recent_dead_wand = "1"
      end
    end
  end

  def activate_wands
    # respond "DEBUG activate_wands"
    result = dothistimeout "wave my #{$huntpro_current_wand_noun} at #{$huntpro_current_target}", 3, /Cast Roundtime|You wave to a|PLACEHOLDER|What were you referring/

    if result =~ /Cast Roundtime/
    # respond "DEBUG Cast Roundtime from wand"
    # Wand is still Active
    elsif result =~ /You wave to a/
      # respond "DEBUG You are not holding a wand, let's reset wand name."
      $huntpro_current_wand_name = "0"
    elsif result =~ /What were you referring/
      # respond "DEBUG You are not holding a wand, let's reset wand name."
      $huntpro_current_wand_name = "0"
    else
      Huntpro_toolkit.dispose_wand
    end
  end

  def upkeep_quartz
    if $huntpro_my_style != "9"
      nil

    elsif $huntpro_noquartz == "1"
      # respond "no quartz feature is turned on."
      nil

    else
      if Spell.active? "1711"
        # respond "1711 is active."
        nil
      else
        result = dothistimeout "get my quartz orb", 5, /Get what|You remove a/

        if result =~ /Get what/
          $huntpro_noquartz = "1"
          Huntpro_toolkit.top_menu
          respond "#{Char.name}, you don't have any quartz orbs for this hunt.  I'll stop trying to find them."
          respond
          respond "You can disable the quartz orb feature with ;huntpro setup change noquartz 1 "
          Huntpro_toolkit.bottom_menu
        else
          fput "rub my quartz orb"
          fput "stow my quartz orb"
          waitrt?
          waitcastrt?
          sleep 1
        end
      end
    end
  end

