				Settings[Char.name]['huntpro_setting_equipment_secondary_weapon']	= "None"
			elsif result =~ /secondary weapon\: (.*) \(/
				Settings[Char.name]['huntpro_setting_equipment_secondary_weapon']	= "#{$1}"
			end
			
			
			result = dothistimeout "ready list", 2, /ranged weapon\: \(none\) \(|ranged weapon\: (.*) \(/
			if result =~ /ranged weapon\: \(none\) \(/
				Settings[Char.name]['huntpro_setting_equipment_primary_ranged']	= "None"
			elsif result =~ /ranged weapon\: (.*) \(/
