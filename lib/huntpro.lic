					Settings[Char.name]['huntpro_setting_equipment_sack_loot']	= "#{Vars.clothingsack}"
					Settings[Char.name]['huntpro_setting_equipment_sack_overflow']	= "#{Vars.clothingsack}"
					Settings[Char.name]['huntpro_setting_equipment_sack_wand']	= "#{Vars.clothingsack}"
