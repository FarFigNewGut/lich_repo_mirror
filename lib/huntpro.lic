huntpro_update_check = proc {

	#These settings will activate upon launch of Huntpro 11.100 or equipment/all reset.
	
	if huntpro_setting_pending_update != "11.100"
		huntpro_reset.call
	end
	
	
	
		#Detect Society & Equipment
				
		if huntpro_setting_last_equipment_update == "0"
		
			Huntpro_toolkit.top_menu
			respond "#{Char.name}, I need to detect some settings. Please stand by. Thanks!"
			Huntpro_toolkit.bottom_menu
			sleep 3
		
			#Check your Society

			if huntpro_setting_character_society =~ /Voln|Gos|Col|None/
					nil
			else
				result = dothistimeout "society", 2, /Voln|Sunfist|Council|society at this/
					if result =~ /society at this/
						Settings[Char.name]['huntpro_setting_character_society']	= "None"
					elsif result =~ /Council/
						Settings[Char.name]['huntpro_setting_character_society']	= "Col"
					elsif result =~ /Sunfist/
						Settings[Char.name]['huntpro_setting_character_society']	= "Gos"
					elsif result =~ /Voln/
						Settings[Char.name]['huntpro_setting_character_society']	= "Voln"
					end
					
					
			end
							
			result = dothistimeout "ready list", 2, /shield\: \(none\) \(|shield\: (.*) \(/
			if result =~ /shield\: \(none\) \(/
				Settings[Char.name]['huntpro_setting_equipment_primary_shield']	= "None"
			elsif result =~ /shield\: (.*) \(/
				Settings[Char.name]['huntpro_setting_equipment_primary_shield']	= "#{$1}"
			end
			
			
			result = dothistimeout "ready list", 2, /weapon\: \(none\) \(|weapon\: (.*) \(/
			if result =~ /weapon\: \(none\) \(/
				Settings[Char.name]['huntpro_setting_equipment_primary_weapon']	= "None"
			elsif result =~ /weapon\: (.*) \(/
				Settings[Char.name]['huntpro_setting_equipment_primary_weapon']	= "#{$1}"
			end
			
			
