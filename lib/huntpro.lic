huntpro_status_check = proc {
  if !checkdead
    nil
  else
    Huntpro_toolkit.top_menu
    respond "#{Char.name}, I'm very sorry, I see you died. If ;huntpro caused your death, please share this feedback."
    Huntpro_toolkit.bottom_menu
    exit
  end

  if checkmind == "must rest"
    if huntpro_setting_hunt_while_fried != "0"
      nil
    else
      if $huntpro_extra_fried <= 60
        $huntpro_extra_fried = $huntpro_extra_fried + 1
      elsif $huntpro_extra_fried >= 61
        $huntpro_return_why = "Your mind is full and you must rest."
        $huntpro_action = 99
      end
    end
  end

  if checkmind == "saturated"
    if huntpro_setting_hunt_while_fried != "0"
      nil
    else
      if $huntpro_extra_fried <= 60
        $huntpro_extra_fried = $huntpro_extra_fried + 1
      elsif $huntpro_extra_fried >= 61
        $huntpro_return_why = "Your mind is saturated and you must rest."
        $huntpro_action = 99
      end
    end
  end

  # Mana Traps
  if $huntpro_group_ai == "1"
    if percentmana >= 50
      nil

    elsif $huntpro_deedmana == "1" && $huntpro_deedmana_zonk == "0"
      Huntpro_toolkit.deed_mana
    elsif percentmana >= 25 && percentmana <= 49 && $huntpro_group_sharemana != "0"

      if $huntpro_mana_cooldown == 0
        $huntpro_mana_cooldown = 25
        $huntpro_group_mana_target = "#{Char.name}"
        fput "whisper group #{$huntpro_group_mana_target} MANA" unless $huntpro_disable_mana == "1"
        sleep 0.5
      elsif $huntpro_mana_cooldown > 0
        $huntpro_mana_cooldown = $huntpro_mana_cooldown - 1
      end
    end

    # Deedmana Goes here
    Huntpro_toolkit.deed_mana

    # Wrack Check Goes Here
    Huntpro_toolkit.wrack_checker if $huntpro_my_society == "Col" && percentmana <= 24

    # Wand Use Goes Here
    if $huntpro_use_wands == "0"

      if huntpro_setting_130_fog != "0" && Stats.level >= 30 && checkmana >= 30 && checkmana <= 41
        $huntpro_return_why = "You have low mana." unless $huntpro_disable_mana == "1"
        $huntpro_action = 99 unless $huntpro_disable_mana == "1"

      elsif $huntpro_style9_arcaneblast == "0" && percentmana <= 13
        $huntpro_return_why = "You have low mana" unless $huntpro_disable_mana == "1"
        $huntpro_action = 99 unless $huntpro_disable_mana == "1"

      # Group Mode Arcane Blast as Leader
      elsif $huntpro_style9_arcaneblast == "1"

        if Stats.level <= 2 && percentmana <= 25
          # respond "Debug Output - You'll use 1700 this round."
          $huntpro_style9_blastround = "1"
        elsif Stats.level <= 2 && percentmana > 25
          # respond "Debug Output - Resuming normal spell choices."
          $huntpro_style9_blastround = "0"
        elsif Stats.level >= 3 && percentmana <= 13
          # respond "Debug Output - You'll use 1700 this round."
          $huntpro_style9_blastround = "1"
        elsif Stats.level >= 3 && percentmana > 13
          # respond "Debug Output - Resuming normal spell choices."
          $huntpro_style9_blastround = "0"
        end
      end
    else
      if percentmana <= 30
        # Huntpro_toolkit.use_wands
        $huntpro_combat_wands = "1"
      elsif percentmana >= 31
        if checkleft == "wand"
          fput "put my wand in my #{Vars.wandsack}"
        elsif checkright == "wand"
          fput "put my wand in my #{Vars.wandsack}"
        elsif checkleft == "rod"
          fput "put my wand in my #{Vars.wandsack}"
        elsif checkright == "rod"
          fput "put my wand in my #{Vars.wandsack}"
        end
        $huntpro_combat_wands = "0"
      else
        nil
      end
    end
  else

    # Deedmana Goes here
    Huntpro_toolkit.deed_mana

    # Wrack Check Goes Here
    Huntpro_toolkit.wrack_checker if $huntpro_my_society == "Col" && percentmana <= 24

    # Wand Use Goes Here
    if $huntpro_use_wands == "0"

      # Style 9 Arcane Blast Toggles Go Here

      if huntpro_setting_130_fog != "0" && Stats.level >= 30 && checkmana >= 30 && checkmana <= 41
        $huntpro_return_why = "You have low mana." unless $huntpro_disable_mana == "1"
        $huntpro_action = 99 unless $huntpro_disable_mana == "1"

      elsif $huntpro_style9_arcaneblast == "0" && percentmana <= 13
        $huntpro_return_why = "You have low mana." unless $huntpro_disable_mana == "1"
        $huntpro_action = 99 unless $huntpro_disable_mana == "1"

      elsif $huntpro_style9_arcaneblast == "1"

        if Stats.level <= 2 && percentmana <= 25
          # respond "Debug Output - You'll use 1700 this round."
          $huntpro_style9_blastround = "1"
        elsif Stats.level <= 2 && percentmana > 25
          # respond "Debug Output - Resuming normal spell choices."
          $huntpro_style9_blastround = "0"
        elsif Stats.level >= 3 && percentmana <= 13
          # respond "Debug Output - You'll use 1700 this round."
          $huntpro_style9_blastround = "1"
        elsif Stats.level >= 3 && percentmana > 13
          # respond "Debug Output - Resuming normal spell choices."
          $huntpro_style9_blastround = "0"
        end
      end
    else
      if percentmana <= 30
        # Huntpro_toolkit.use_wands
        $huntpro_combat_wands = "1"
      elsif percentmana >= 31
        if checkleft == "wand"
          fput "put my wand in my #{Vars.lootsack}"
        elsif checkright == "wand"
          fput "put my wand in my #{Vars.lootsack}"
        elsif checkleft == "rod"
          fput "put my rod in my #{Vars.lootsack}"
        elsif checkright == "rod"
          fput "put my rod in my #{Vars.lootsack}"
        end
        $huntpro_combat_wands = "0"
      else
        nil
      end
    end
  end



  if percenthealth <= 50
    # respond "Debug - Health is less than 50"
    if $huntpro_group_ai == "1" && $huntpro_group_empath != "0"
      if $huntpro_heal_cooldown == 0
        $huntpro_heal_cooldown = 1
        fput "whisper group #{$huntpro_group_empath} please heal us."
        Script.pause('huntpro')
      elsif $huntpro_heal_cooldown == 1
        $huntpro_heal_cooldown = 0
      end
    else
      if Stats.prof == "Empath" && checkmana >= 10 && Spell[1101].known?
        fput "cure"
      elsif $huntpro_use_herbs == "1"
        $huntpro_heal_toggle = 1
        Huntpro_toolkit.combat_use_herbs
      else
        $huntpro_return_why = "You have low health."
        $huntpro_action = 99
      end
    end
  end

  if percentstamina <= $huntpro_value_stamina.to_i
    if huntpro_setting_disable_stamina != "0"
      nil
    else
      $huntpro_return_why = "You have low stamina."
      $huntpro_action = 99
    end

  end

  if percentspirit <= 10
    $huntpro_return_why = "You have low spirit."
    $huntpro_action = 99
  end

  if percentencumbrance >= $huntpro_value_encumbrance.to_i
    if huntpro_setting_disable_encumbrance != "0"
      nil
