				$huntpro_id_placeholder = $huntpro_creature_id_sev_1_attack[0]
				$huntpro_name_placeholder = $huntpro_creature_name_sev_1_attack[0]
				
			elsif $huntpro_current_id_weak_targets.length >= 1
