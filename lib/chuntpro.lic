crossbow = proc {


until checkkneeling
		fput "kneel"
		pause 1
	end
	
	if Skills.multiopponentcombat >= 5
		mstrike_routine.call
	end
	
	if setting_stay_offensive.active?
		nil
	else
		fput "stance offensive"
	end
	result = dothistimeout "fire ##{target}", 2, /no effect|Roundtime/
	if result =~ /no effect/
		return_why = "You don\'t have an active bless to hunt undead."
		respond "You don\'t have an active bless to hunt undead."
		respond "Returning to town, obtain a bless and start script again."
		do_client ";#{setting_loot_script}"
		wait_while {running? "#{setting_loot_script}"}
		safe_room.call
		script_kill.call
	elsif result =~ /Roundtime/
		waitrt?
		if kill_count >= 1
			u += 1
		else
			u = 10
		end
	else
		waitrt?
		loot_script.call
	end


}

