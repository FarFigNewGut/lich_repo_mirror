status_check = proc {
if checkdead
	script_kill.call
end

stand_check.call
stance_check.call

if script.vars[2] == "escort"
	nil
else
	cloud_check.call
	health_check.call
	wound_check.call
	stun_check.call
	fried_check.call
	encumbrance_check.call
	poison_disease_check.call
	
end
}


