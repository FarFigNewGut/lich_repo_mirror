	fput "symbol supremacy" unless Spell.active? "9816"
	
end
	

if setting_ignore_rapidfire.active?
	nil
else	
	Spell[515].cast() if Spell[515].known? && Spell[515].affordable? && Char.level >= 20 unless Spell.active? "599"
