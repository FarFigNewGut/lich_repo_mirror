disk_pool = proc {

result = dothistimeout "look in #{box_container3}", 2, /nothing in there|nothing in the disk|coffer|strongbox|box|chest|trunk/
	if result =~ /nothing in there|nothing in the disk/
		more_disk_boxes = 0
	elsif result =~ /coffer/
		fput "get coffer from #{box_container3}"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		sleep 2
	elsif result =~ /strongbox/
		fput "get strongbox from #{box_container3}"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		sleep 2
	elsif result =~ /box/
		fput "get box from #{box_container3}"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		sleep 2
	elsif result =~ /chest/
		fput "get chest from #{box_container3}"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		sleep 2
	elsif result =~ /trunk/
		fput "get trunk from #{box_container3}"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		sleep 2
	end
}

pool_deposit_count = proc {
pause 5
current_boxes = Array.new

box_container = GameObj.inv.find { |i|
 
	i.noun == Vars.boxsack

}

if setting_overflow_container2.nil?
	nil
else	
	box_container2 = GameObj.inv.find { |i|
 
	i.noun == setting_overflow_container2.downcase
	}
end


box_container.contents.each { |i|
	if i.type == "box"
	current_boxes.push(i.id) unless current_boxes.include? (i.id)
	end
}

if setting_overflow_container2.nil?
	nil
else	
	box_container2.contents.each { |i|
	if i.type == "box"
	current_boxes.push(i.id) unless current_boxes.include? (i.id)
	end
	}
end

how_many_boxes = current_boxes.length.to_i

how_many_boxes.to_i.times {
fput "get ##{current_boxes[0]}"
current_boxes.delete_at(0)
result = dothistimeout "give #{pool_npc} #{setting_pool_tip} percent", 2, /doesn't appear to be|seconds to confirm/
	if result =~ /doesn't appear to be/
		fput "swap"
		sleep 1
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		
	elsif result =~ /seconds to confirm/
		fput "give #{pool_npc} #{setting_pool_tip} percent"
		sleep 1		
	end
}


GameObj.loot.each { |i|
 if i.name =~ /#{Char.name} disk/
	box_container3 = "#{Char.name} disk"
elsif i.name =~ /#{Char.name} coffin/
	box_container3 = "#{Char.name} coffin"
end
}

if box_container3 == "Disk"
	nil
else
	more_disk_boxes = 1
	until more_disk_boxes == 0
	disk_pool.call
	sleep 2
	end	
end
}

