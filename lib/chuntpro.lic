huntpro_withdraw_silvers = proc {

	result = dothistimeout "withdraw 10000 silver", 5, /carefully records|makes some marks|through the books/
	
	if result =~ /carefully records|makes some marks/
			nil			
	elsif result =~ /through the books/
			$chuntpro_bank_nosilvers = 1
			top_menu.call
			respond "#{Char.name}, you do not have enough money in bank."
			respond
			respond "Skipping the rest of this box round."
			bottom_menu.call
			sleep 3
	end

}

