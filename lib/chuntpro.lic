#MOC Procedure
moc_combat_routine = proc {

if Char.prof == "Wizard"
	if Skills.spellaiming >= 1
		if kill_count >= 3 && Char.level >= 18
			s518.call if Spell[518].known? && Char.level >= 18
		elsif kill_count >= 2
			if percentmana >= 50
				if knows_major_acid == 1
					s904.call if Spell[904].known? && Char.level >= 4
				elsif knows_minor_steam == 1
					s903.call if Spell[903].known? && Char.level >= 3
				end
			end
		end
	end

elsif Char.prof == "Empath"
	if Skills.spellaiming >= 1
		if kill_count >= 2
			if percentmana >= 50 && Char.level >= 18 && Skills.slsummoning >= 20
				sfireweb.call unless my_hunting_area =~ /terasltroll|teraspyrothag|terastsark/
			elsif percentmana >= 50 && Char.level >= 11 && Spell[111].known?
				s111.call unless my_hunting_area =~ /terasltroll|teraspyrothag|terastsark/
			end
		end
	end
	
elsif Char.prof == "Cleric"
	if Skills.spellaiming >= 1
		if kill_count >= 2
			if percentmana >= 50 && Char.level >= 18 && Skills.slsummoning >= 20
				sfireweb.call unless my_hunting_area =~ /terasltroll|teraspyrothag|terastsark/
			elsif percentmana >= 50 && Char.level >= 11 && Spell[111].known?
				s111.call unless my_hunting_area =~ /terasltroll|teraspyrothag|terastsark/
			end
		end
	end
	
elsif Char.prof == "Sorcerer"
	if Skills.spellaiming >= 1
		if kill_count >= 2
				if percentmana >= 50 && Char.level >= 18 && Skills.slsummoning >= 20
				sfireweb.call unless my_hunting_area =~ /terasltroll|teraspyrothag|terastsark/
			elsif percentmana >= 50 && Char.level >= 13 && Spell[713].known?
				s713.call unless my_hunting_area =~ /wlcarceris/
			elsif percentmana >= 50 && Char.level >= 11 && Spell[111].known?
				s111.call unless my_hunting_area =~ /terasltroll|teraspyrothag|terastsark/
			end
		end
	end
end
}

