pool_trip = proc {

pool_location_check.call

if $chuntpro_bank_nosilvers == 1
	fput "stow all"
	pool_mode = 0
	Script.run('go2', 'bank')
end
	
if pool_mode == 1
	pool_deposit_count.call
	pool_pickup.call
	until get_more_boxes == 0
		pool_pickup.call
	end
	Script.run('go2', 'bank')
end

fput "deposit all"

}

