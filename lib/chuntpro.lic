element_check = proc {


#Wizard Element Detection

if (Skills.elair >= Skills.elearth && Skills.elair >= Skills.elfire && Skills.elair >= Skills.elwater)
	wizard_type = "Air"
elsif (Skills.elearth >= Skills.elair && Skills.elearth >= Skills.elfire && Skills.elearth >= Skills.elwater)
	wizard_type = "Earth"
elsif (Skills.elfire >= Skills.elair && Skills.elfire >= Skills.elearth && Skills.elfire >= Skills.elwater)
	wizard_type = "Fire"
elsif (Skills.elwater >= Skills.elair && Skills.elwater >= Skills.elfire && Skills.elwater >= Skills.elearth)
	wizard_type = "Water"
else
	wizard_type = "General"
end

if Skills.elfire >= 20
	knows_518_fire = 1
	knows_minor_steam = 1
end

if Skills.elwater >= 20
	knows_518_cold = 1
	knows_518_water = 1
end

if Skills.elearth >= 20
	knows_518_earth = 1
end

if Skills.elair >= 20
	knows_518_air = 1
end

if Skills.elair >= 10 && Skills.elwater >= 10
	knows_518_lightning = 1
end

if Skills.elearth >= 10 && Skills.elwater >= 10
	knows_518_acid = 1
end

if Skills.elfire >= 10 && Skills.elwater >= 10
	knows_518_steam = 1
end

if Skills.elwater >= 10
	knows_minor_cold = 1
end

if Skills.elwater >= 30
	knows_major_acid = 1
end


}

