	Script.run('repository', 'download useherbs.lic') if !Script.exists? ('useherbs')
	Script.run('repository', 'download waggle.lic') if !Script.exists? ('waggle')
	Script.run('repository', 'download dropbox.lic') if !Script.exists? ('dropbox')
	respond
	respond "Hi #{Char.name}, ;chuntpro has been updated, please restart the script. Thanks!"
	respond	
	Script.kill('chuntpro') if Script.running? "chuntpro"
	Script.kill('cintel_huntpro') if Script.running? "cintel_huntpro"	
	exit				
end

#;chuntpro help
if script.vars[1] =~ /huntpro_help_menus/
	huntpro_help_menus.call
end

#Things to watch for while ;chuntpro runs
while line = get

	@chuntpro_death.call
	if line =~ /RECOVER ITEM command/
		@chuntpro_disarm_alert.call
	end
	
	
	if line =~ /obvious signs of someone hiding/
		@chuntpro_hide_watch_alert.call
	end
	
		#Stone Troll Ledge Trap
	if line =~ /Accept your fate/
			Script.pause('huntpro')
	end
	
	#Stone Troll Ledge Trap
	if line =~ /tossed about like a rag/
			Script.unpause('huntpro')
	end
	
		#Caedera Trap
	if line =~ /ground shakes violently as a caedera explodes from the ground/
		sleep 3	
	#Boil Trap
	elsif line =~ /burst from the ground|explodes from the ground|earth cracks/
		Script.pause('huntpro')
		run
		Script.unpause('huntpro')
	end	
	
	
end