@chuntpro_current_version = proc {

@chuntpro_top_menu.call
respond "#{$chuntpro_current_version}"
@chuntpro_bottom_menu.call

}

@chuntpro_death = proc {

if !checkdead
		nil
	else
		@chuntpro_top_menu.call
		respond "#{Char.name}, I'm very sorry, I see you died. Please share this feedback if I did something wrong."
		@chuntpro_bottom_menu.call
		Script.kill('huntpro')
		Script.kill('cintel_huntpro')
end
}

@chuntpro_disarm_recovery = proc {

@chuntpro_death.call

result = dothistimeout "recover item", 2, /and recover it/
		if result =~ /and recover it/
			$chuntpro_item_lost = 0
			fput "stand"
			Script.unpause('huntpro')
		end
}

@chuntpro_disarm_alert = proc {

#Disarm Recovery
	
		$chuntpro_room_dropped = Room.current.id
		$chuntpro_item_lost = 1
		Script.pause('huntpro')
		respond
		respond
		respond "Attention - Attention - You have lost an item."
		respond "Attention - Attention - You have lost an item."
		respond "Attention - Attention - You have lost an item."
		respond "Attention - Attention - You have lost an item."
		respond
		respond
		respond
		respond "The item was lost in room #{$chuntpro_room_dropped}."
		respond
		respond		
		
		
		if $chuntpro_ignore_disarm == 1
			Script.unpause('huntpro')
		else
			waitrt?
			Spell[213].cast() if Spell[213].known? && Spell[213].affordable?
			fput "kneel"
			until  $chuntpro_item_lost == 0
			@chuntpro_disarm_recovery.call
			end		
		end


}

