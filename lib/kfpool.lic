=begin
	# version: 1
	# Uses foreach to deposit boxes and then runs eloot
	# Helps some users who run into issues when the locksmith pool has too many visitors
	# Author: Zegres
=end
	Script.run('go2','bank')
	fput "withdraw 8000"
	Script.run('go2','locksmithpool')
	fput "stow all"
	Script.run('foreach','box in inv;get item;give worker 100;give worker 100;') 
	Script.run('go2','bank')
	fput "deposit all"
	Script.run('eloot','sell')

