def aoe_att
	if ((GameObj.targets.count > @osaaoe_if.to_i) && ((!stunned?) && (!webbed?) && (!bound?) && (!checkdead)))	
		if @osaaoe == 0
			return
		elsif @osaaoe == 1
			bull_rush
		elsif @osaaoe == 2
			clash
		elsif @osaaoe == 3
			cyclone
		elsif @osaaoe == 4
			pin_down
		elsif @osaaoe == 5
			flare_gloves_pound
		elsif @osaaoe == 6
			pulverize
		elsif @osaaoe == 7
			shield_throw
		elsif @osaaoe == 8
			shield_trample
		elsif @osaaoe == 9
			whirling_blade
		elsif @osaaoe == 10
			whirlwind
		elsif @osaaoe == 11
			volley
		end
	end
end

