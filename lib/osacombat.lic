	add_checkbox(@OSA_COM_TABLE5, 0,  "Arcanist\'s Blade", 'You enter a heightened state for 30 seconds. You gain 5/10/15/20/25 Attack Strength and 3/6/9/12/15% Damage Factor, but each attack costs 5 mana and 5 stamina. Upon activation you gain Mana Sever, preventing you from receiving mana from your fellow adventurers for 2 minutes.', '$osa_gemstone_arcanists_blade')
	add_label_entry(@OSA_COM_TABLE5, "Enemy Threshold:", 'Only activates Arcanist\'s Ascendancy if the enemy count is above this amount. Default setting is 0.', '$osa_gemstone_activate_arcanists_blade_enemy_if', 1, 2, 2, 3, '0', true)
	add_label_entry(@OSA_COM_TABLE5, "Mana Threshold:", 'Only activates Arcanist\'s Ascendancy if your mana points are above this amount. Default setting is 50.', '$osa_gemstone_activate_arcanists_blade_mana_if', 1, 2, 2, 3, '50', true)
	add_label_entry(@OSA_COM_TABLE5, "Stamina Threshold:", 'Only activates Arcanist\'s Ascendancy if your stamina points are above this amount. Default setting is 50.', '$osa_gemstone_activate_arcanists_blade_stamina_if', 1, 2, 2, 3, '50', true)
	add_checkbox(@OSA_COM_TABLE5, 0,  "Arcanist\'s Will", 'You enter a heightened state for 30 seconds. You gain 3/6/9/12/15 Casting Strength and your successful warding spells receive an endroll bonus of 2/4/6/8/10, but each attack costs 5 mana and 5 stamina. Upon activation you gain Mana Sever, preventing you from receiving mana from your fellow adventurers for 2 minutes.', '$osa_gemstone_arcanists_will', true)
