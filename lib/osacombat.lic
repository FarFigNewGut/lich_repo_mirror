	$yaml_data = YAML.load_file(full_path, permitted_classes: [Time, Symbol])
	if @OSA_COM_ENTRY[key].to_s =~ /CheckButton/
        $yaml_data[key] = @OSA_COM_ENTRY[key].active?
	elsif @OSA_COM_ENTRY[key].to_s =~ /ComboBox/
        $yaml_data[key] = @OSA_COM_ENTRY[key].active
	elsif @OSA_COM_ENTRY[key].to_s =~ /TextView/
		$yaml_data[key] = @OSA_COM_ENTRY[key].buffer.text.gsub("\n","||")
	else
		$yaml_data[key] = @OSA_COM_ENTRY[key].text.strip
    end
	File.open(full_path, 'w') do |file|
			file.write($yaml_data.to_yaml)
	end
	Settings[Char.name]['yamlpath']	= "#{full_path}"
	Settings.save
	$combat_data = YAML.load_file("#{DATA_DIR}/#{XMLData.game}/#{Char.name}/osa_vars.yaml", permitted_classes: [Time, Symbol])
end
$combat_data.each do |key, var|
