def stand(stand_command = nil)
    echo "stand" if $bigshot_debug
    return if (stand_command =~ /^(?:fire|kneel|hide)/i && kneeling? && checkleft =~ /^(?:arbalest|kut'ziko|crossbow|kut'zikokra)$/)
    until (standing?)
      change_stance('defensive')
      osa_put 'stand'
    end
end

