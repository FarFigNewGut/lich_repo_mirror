deeds = "Unk"
sting = "Unk"
deaths = "Unk"
res = Lich::Util.quiet_command('exp', /Level:/, /ATPs/)
res.each do |line|
  matchSting = line.match(%r{Death's Sting: (.*)})
  sting = matchSting[1] if matchSting
  matchDeeds = line.match(%r{Deeds: (.*)})
  deeds = matchDeeds[1] if matchDeeds
  matchDeaths = line.match(%r{Recent Deaths: (.*)})
  deaths = matchDeaths[1] if matchDeaths
end

respond "        Deeds: #{deeds}"
respond "Death's Sting: #{sting}"
respond "Recent Deaths: #{deaths}"
