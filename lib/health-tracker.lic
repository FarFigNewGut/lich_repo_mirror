toggle_upstream

CharSettings['critter_info'] = critter_info unless CharSettings['critter_info']

silence_this = proc{
	action = proc { |server_string|
		if server_string =~ /Please rephrase that command\./
			DownstreamHook.remove("#{script.name}_silence_this")
			nil
		else
			server_string
		end
	}
		DownstreamHook.add("#{script.name}_silence_this", action)
}

before_dying { DownstreamHook.remove("#{script.name}_silence_this") }

Thread.new{
	loop{
		command = upstream_get
		if command =~ /stats all/
			silence_this.call
			CharSettings['critter_info'].each { |key,value|
				respond "#{key}: Max health: #{CharSettings['critter_info'][key][:maxhealth]}, 20%: #{CharSettings['critter_info'][key][:e20]}, 23%: #{CharSettings['critter_info'][key][:e23]}, 26%: #{CharSettings['critter_info'][key][:e26]}, 29%: #{CharSettings['critter_info'][key][:e29]}, 32%: #{CharSettings['critter_info'][key][:e32]}, 35%: #{CharSettings['critter_info'][key][:e35]}"
			}	
		elsif command =~ /stats new/
			silence_this.call
			message = nil
			new_information = nil
			CharSettings['critter_info'].each { |key,value|
				if (CharSettings['critter_info'][key][:maxhealth] != critter_info[key][:maxhealth]) || (CharSettings['critter_info'][key][:e20] != critter_info[key][:e20]) || (CharSettings['critter_info'][key][:e23] != critter_info[key][:e23]) || (CharSettings['critter_info'][key][:e26] != critter_info[key][:e26]) || (CharSettings['critter_info'][key][:e29] != critter_info[key][:e29]) || (CharSettings['critter_info'][key][:e32] != critter_info[key][:e32]) || (CharSettings['critter_info'][key][:e35] != critter_info[key][:e35])
					new_information = true
					message = "#{key}: "
					if (CharSettings['critter_info'][key][:e20] != critter_info[key][:e20])
						message += " 20%: #{CharSettings['critter_info'][key][:e20]},"
					end
					if (CharSettings['critter_info'][key][:e23] != critter_info[key][:e23])
						message += " 23%: #{CharSettings['critter_info'][key][:e23]},"
					end
					if (CharSettings['critter_info'][key][:e26] != critter_info[key][:e26])
						message += " 26%: #{CharSettings['critter_info'][key][:e26]},"
					end
					if (CharSettings['critter_info'][key][:e29] != critter_info[key][:e29])
						message += " 29%: #{CharSettings['critter_info'][key][:e29]},"
					end
					if (CharSettings['critter_info'][key][:e32] != critter_info[key][:e32])
						message += " 32%: #{CharSettings['critter_info'][key][:e32]},"
					end
					if (CharSettings['critter_info'][key][:e35] != critter_info[key][:e35])
						message += " 35%: #{CharSettings['critter_info'][key][:e35]},"
					end
					respond message.chop
				end
			}
			if new_information == nil
				echo "No new critter information has been observed yet."
			end
		elsif command =~ /stats update/
			silence_this.call
			CharSettings['critter_info'] = critter_info
			echo "All data has been updated."
		elsif command =~ /stats cal/
			silence_this.call
			CharSettings['critter_info'].each { |key,value|
				message = nil
				temp20 = CharSettings['critter_info'][key][:e20]
				temp23 = CharSettings['critter_info'][key][:e23]
				temp26 = CharSettings['critter_info'][key][:e26]
				temp29 = CharSettings['critter_info'][key][:e29]
				temp32 = CharSettings['critter_info'][key][:e32]
				temp35 = CharSettings['critter_info'][key][:e35]
				tempmin = 0
				tempmax = 0
				finalmin = 0
				finalmax = 0
				if (temp20 != 0)
					tempmin = ((temp20 - 0.5) / 0.20).round
					tempmax = ((temp20 + 0.5) / 0.20).floor
					(finalmin = tempmin) if (finalmin == 0)
					(finalmax = tempmax) if (finalmax == 0)
				end
				if (temp23 != 0)
					tempmin = ((temp23 - 0.5) / 0.23).round
					tempmax = ((temp23 + 0.5) / 0.23).floor
					(finalmin = tempmin) if (finalmin == 0)
					(finalmax = tempmax) if (finalmax == 0)
					(finalmin = tempmin) if (tempmin > finalmin)
					(finalmax = tempmax) if (tempmin < finalmax)
				end
				if (temp26 != 0)
					tempmin = ((temp26 - 0.5) / 0.26).round
					tempmax = ((temp26 + 0.5) / 0.26).floor
					(finalmin = tempmin) if (finalmin == 0)
					(finalmax = tempmax) if (finalmax == 0)
					(finalmin = tempmin) if (tempmin > finalmin)
					(finalmax = tempmax) if (tempmin < finalmax)
				end
				if (temp29 != 0)
					tempmin = ((temp29 - 0.5) / 0.29).round
					tempmax = ((temp29 + 0.5) / 0.29).floor
					(finalmin = tempmin) if (finalmin == 0)
					(finalmax = tempmax) if (finalmax == 0)
					(finalmin = tempmin) if (tempmin > finalmin)
					(finalmax = tempmax) if (tempmin < finalmax)
				end
				if (temp32 != 0)
					tempmin = ((temp32 - 0.5) / 0.32).round
					tempmax = ((temp32 + 0.5) / 0.32).floor
					(finalmin = tempmin) if (finalmin == 0)
					(finalmax = tempmax) if (finalmax == 0)
					(finalmin = tempmin) if (tempmin > finalmin)
					(finalmax = tempmax) if (tempmin < finalmax)
				end
				if (temp35 != 0)
					tempmin = ((temp35 - 0.5) / 0.35).round
					tempmax = ((temp35 + 0.5) / 0.35).floor
					(finalmin = tempmin) if (finalmin == 0)
					(finalmax = tempmax) if (finalmax == 0)
					(finalmin = tempmin) if (tempmin > finalmin)
					(finalmax = tempmax) if (tempmin < finalmax)
				end
				if (tempmin != 0) && (tempmax != 0)
					if (CharSettings['critter_info'][key][:minhealth] != tempmin) || (CharSettings['critter_info'][key][:maxhealth] != tempmax)
						message = "#{key}:"
					end
					if (CharSettings['critter_info'][key][:minhealth] != tempmin)
						message += " Min health: #{tempmin}"
					end
					if (CharSettings['critter_info'][key][:maxhealth] != tempmax)
						message += " Max health: #{tempmax}"
					end
				end
				(respond message) if (message != nil)
			}
		end
	}
}

while line = get
	if line =~ /You trace an intricate sign that contorts in the air while forcefully invoking Pain\.\.\./
		modifier = nil
		damage_done = nil
		result = nil
		critter_name = nil
		critter_in_database = nil
		while line = get
			if line =~ /You gesture at an? (.*)\./
				critter_name = $1
				while line = get
					if line =~ /cringes in pain\./
						modifier = 0.20
					elsif line =~ /shudders in pain\./
						modifier = 0.23
					elsif line =~ /twists in great pain\!/
						modifier = 0.26
					elsif line =~ /is wracked with painful spasms\!/
						modifier = 0.29
					elsif line =~ /shudders and twists in intense pain\!/
						modifier = 0.32
					elsif line =~ /contorts in excruciating agony\!/
						modifier = 0.35
					elsif line =~ /(\d+) points? of damage\!/
						damage_done = $1.to_i
						break
					elsif line =~ /Warded off\!/
						break
					end
				end
			elsif line =~ /You feel the magic of your spell rush away from you\.|You feel the magic of your spell depart\.|Roundtime/
				break
			end
		end
		if (damage_done) && (modifier) && (critter_name)
			result = (damage_done / modifier).floor
			modified_modifier = (modifier * 100)
			CharSettings['critter_info'].each { |key,value|
				if key == critter_name
					critter_in_database = true
					if modifier == 0.20
						CharSettings['critter_info'][key][:e20] = damage_done
					elsif modifier == 0.23
						CharSettings['critter_info'][key][:e23] = damage_done
					elsif modifier == 0.26
						CharSettings['critter_info'][key][:e26] = damage_done
					elsif modifier == 0.29
						CharSettings['critter_info'][key][:e29] = damage_done
					elsif modifier == 0.32
						CharSettings['critter_info'][key][:e32] = damage_done
					elsif modifier == 0.35
						CharSettings['critter_info'][key][:e35] = damage_done
					end
					if CharSettings['critter_info'][key][:maxhealth] == 0
						CharSettings['critter_info'][key][:maxhealth] = result
					end
					echo "#{key} information added to the database."
				end
			}
			if critter_in_database == nil
				echo "#{critter_name} doesn't appear to be listed in the database. Please inform the author of this script (Tgo01/Dreaven) about this so he can add this critter to the database."
			end
			echo "#{damage_done} damage (#{modified_modifier}%) done to #{critter_name}. Total health: #{result}"
		end
	end
end