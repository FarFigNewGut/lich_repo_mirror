def termsearch(args,undead)
	if args.find { |var| var =~ /\bundead\b/i } and undead then true
	elsif args.find { |var| var =~ /\bundead\b/i } and !undead then false
	elsif args.find { |var| var =~ /\bliving\b/i } and !undead then true
	elsif args.find { |var| var =~ /\bliving\b/i } and undead then false
	else true
	end
end

if script.vars.find { |var| var =~ /\bstat\b|\bstats\b/i }
	if script.vars[0] =~ /(\d+)/ then interval = $1.to_i
	else interval = 5
	end
	respond("\nCreature Level Distribution\n\n")
	lvli = 1; total = 0
	while lvli < 110
		lvlf = lvli + (interval-1)
		count = creature_database.find_all { |name,data| data['level'] >= lvli and data['level'] <= lvlf and termsearch(script.vars,data['undead']) }.size
		bar = ""; count.times { bar += "="}
		respond("\t#{lvli}-#{lvlf}\t#{bar} #{count}")
		lvli += interval; total += count
	end
	respond("\n\tTotal\t#{total}\n\n")
elsif script.vars.find { |var| var =~ /\blevel\b/i }
	if script.vars[0] =~ /(\d+)\-(\d+)/ then levelrange = (($1.to_i)..($2.to_i)).to_a
	elsif script.vars[0] =~ /(\d+)/ then levelrange = ((Char.level-$1.to_i)..(Char.level+$1.to_i)).to_a
	else levelrange = ((Char.level-5)..(Char.level+5)).to_a
	end
	respond("\nCreatures in level range: (#{levelrange[0]}-#{levelrange[-1]})\n\n")
	respond("\tLvl : Creatures")
	levelchart = Hash.new; levelrange.each { |level| levelchart[level] = Array.new }
	creature_database.each { |name,data|
		if levelrange.include?(data["level"]) and termsearch(script.vars,data['undead'])
			levelchart[data["level"]].push(name)
		end
	}
	levelchart.sort.each { |level,list|
		if list.size == 0 then list = ["none"]; end
		respond("\t#{level} : #{list.join(", ")}")
	}
	respond("\n")
elsif script.vars.find { |var| var =~ /\bsearch\b/i }
	terms = script.vars[2..-1]
	if script.vars.find { |var| var =~ /\bundead\b/i } then terms.delete(terms.find { |var| var =~ /undead/i })
	elsif script.vars.find { |var| var =~ /\bliving\b/i } then terms.delete(terms.find { |var| var =~ /living/i })
	end
	respond("\nCreaure database search: /#{terms.join("|")}/i\n\n")
	matches = creature_database.find_all { |name,data| name =~ /#{terms.join("|")}/i and termsearch(script.vars,data['undead']) }
	matches.each { |name,data|
		info=""
		if data["undead"] then info = "[Undead"
		else info = "[Living"; end
		if data["corporeal"] then info += ", Corporeal]"
		else info += ", Non-corporeal]"; end
		respond("\t(#{data["level"]}) #{name} #{info}")
		respond("\t\tLink: #{data["url"]}")
		respond("\t\tAreas: #{data["locations"].join(", ")}")
	}
	respond("\n")
else
	respond("\nUsage:\n\n")
	respond("\t;#{script.name} stats <n>\n\t\tDisplays a chart of creature quantities in level ranges (<n> optional, default: 5)")
	respond("\t;#{script.name} level <n>\n\t\tDisplays creatures within n levels of you (<n> optional, default: 5)")
	respond("\t;#{script.name} level <n>-<m>\n\t\tDisplays creatures within the specified level range")
	respond("\t;#{script.name} search <terms>\n\t\tSearches database for creatures with names that contain any of the terms provided")
	respond("\nAdd the term 'undead' or 'living' to restrict output to undead or living creatures.\n\n")
	exit
end

