# quiet
=begin
  
  Taken from https://elanthipedia.play.net/User:Enfo/Scripts/WoundSubstitution.cmd
  Adapted for Stormfront
  
  Creates a healsubs.xml to import custom color highlights for the new HEALTH window
  click Options, Settings, Import, Custom Highlights - Import All
  
  
=end

custom_require.call(%w[drinfomon])

no_pause_all
no_kill_all

class HealSubs
  @@subs = {}

  def self.subs
    @@subs
  end

  def self.clear
    @@subs = {}
  end

  def self.add(target, result)
    @@subs[/#{target}/] = result
  end
end

action = proc do |server_string|
  matches = HealSubs.subs.select { |key, _| key =~ server_string }
  matches.each { |target, result| server_string.gsub!(target, result) }
  server_string
end

DownstreamHook.remove('healsub')
DownstreamHook.add('healsub', action)

before_dying do
  DownstreamHook.remove('healsub')
end

